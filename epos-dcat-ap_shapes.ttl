# ContactPoint optional properties
####
  sh:property [
      sh:path schema:name ;
      sh:datatype xsd:string;
      sh:maxCount 1 ;
    ] ;
  sh:property [
      sh:path schema:email ;
      sh:datatype xsd:string;
    ] ;
  sh:property [
       sh:path schema:availableLanguage ;
       sh:datatype xsd:string;
     ] ;
  sh:property [
      sh:path schema:telephone ;
      sh:datatype xsd:string;
    ] ;
.
