  owl:imports <http://www.w3.org/ns/oa#> ;
  owl:imports org: ;
  owl:imports foaf: ;
  owl:versionInfo "0.9" ;
  foaf:homepage <http://www.epos-eu.org/dcat-ap> ;
.
  ####
  # Classes 
  ###

epos:Equipment
  a owl:Class ;
  rdfs:comment "A resource representing a generic equipment. E.g. gps sensor, seismic station's channel"@en  ;
  rdfs:isDefinedBy <http://www.epos-eu.org/epos/dcat-ap> ;
  rdfs:label "Equipment"@en  ;
  rdfs:subClassOf rdfs:Resource ;
.
epos:Facility
  a owl:Class ;
  rdfs:comment "A resource representing a Facility. E.g. seismic station"@en  ;
  rdfs:isDefinedBy <http://www.epos-eu.org/epos/dcat-ap> ;
  rdfs:label "Facility"@en  ;
  rdfs:subClassOf rdfs:Resource ;
.


##redefining schema:Organization and schema:Person 
schema:Organization a owl:Class;
    owl:equivalentClass foaf:Organization; 
    rdfs:label "Organization"@en;
.
schema:Person a owl:Class;
    owl:equivalentClass foaf:Person;
    rdfs:label "Person"@en;   
.
#aligning vcard and schema 
schema:ContactPoint a owl:Class;
    owl:equivalentClass vcard:Kind;
    rdfs:label "ContactPoint"@en;   
.

epos:Publication
  a owl:Class ;
  rdfs:comment "EPOS specialisation of schema:CreativeWork"@en  ;    
  rdfs:isDefinedBy <http://www.epos-eu.org/epos/dcat-ap> ;
  rdfs:label "Publication"@en  ;   
  rdfs:subClassOf schema:CreativeWork ;
.

epos:WebService
  a owl:Class ;
  rdfs:comment "It represents a WebService API "@en  ;  
  rdfs:subClassOf schema:Service;
  rdfs:subClassOf rdfs:Resource ;
  rdfs:label "WebService"@en  ;     
  rdfs:isDefinedBy <http://www.epos-eu.org/epos/dcat-ap> ;
.

  ####
  # Properties 
  ###

##We declare the equivalence of properties of dcat and schema.org
dcat:contactPoint owl:equivalentProperty schema:ContactPoint .
dcat:keyword owl:equivalentProperty schema:keywords .

##We extend range and domain of dcat:contactPoint
dcat:contactPoint 
 rdf:type owl:ObjectProperty ;
  rdfs:domain schema:Organization ;
  rdfs:domain schema:Person ;
  rdfs:domain epos:Equipment ;
  rdfs:domain schema:SoftwareApplication ;
  rdfs:domain schema:SoftwareSourceCode;
  rdfs:domain epos:WebService ;
  rdfs:domain foaf:Project ;
  rdfs:domain epos:Facility ;
 rdfs:range vcard:Kind;
 rdfs:range schema:ContactPoint ;
 #rdfs:range schema:Person ;
 .
 
epos:associatedProject
  rdf:type owl:ObjectProperty;
  rdfs:domain schema:Organization ;
  rdfs:range foaf:Project ;
.

epos:annotation
  rdf:type owl:ObjectProperty ;
  rdfs:domain rdfs:Resource ;
  rdfs:domain foaf:Agent ;
  rdfs:range oa:Annotation ;
.

dcat:theme
  rdf:type owl:ObjectProperty ;
  rdfs:comment "The main category/domain of the referred resource. It is an extension of dcat:theme"@en  ;
  rdfs:domain rdfs:Resource ;
  rdfs:domain dcat:Dataset ;
  rdfs:domain dcat:Distribution ;
#  rdfs:domain epos:Facility ;
 # rdfs:domain epos:WebService ;
  rdfs:range skos:Concept ;
.
epos:dynamicRange
  rdf:type owl:DatatypeProperty ;
  rdfs:comment "This property contains the full scale measuring ability, in nT (unit and value)"@en  ;
  rdfs:domain epos:Equipment ;
  rdfs:range rdfs:Literal ;
.
epos:filter
  rdf:type owl:DatatypeProperty ;
  rdfs:comment "This property describes the filter that the instrument uses to produce data"@en  ;
  rdfs:domain epos:Equipment ;
  rdfs:range rdfs:Literal ;
.
epos:samplePeriod
  rdf:type owl:DatatypeProperty ;
  rdfs:comment "This property contains the sample period in mS"@en  ;
  rdfs:domain epos:Equipment ;
  rdfs:range rdfs:Literal ;
.

epos:financialContact
  rdf:type owl:ObjectProperty ;
  rdfs:subPropertyOf dcat:contactPoint;
  rdfs:domain schema:Organization ;
  rdfs:range schema:Person ;
.
epos:legalContact
  rdf:type owl:ObjectProperty ;
  rdfs:subPropertyOf dcat:contactPoint;
  rdfs:domain schema:Organization ;
  rdfs:range schema:Person ;
.
epos:manager
  rdf:type owl:ObjectProperty ;
  rdfs:domain epos:Facility ;
  rdfs:range schema:Person ;
.
epos:orientation
  rdf:type owl:DatatypeProperty ;
  rdfs:comment "This property describes how the instrument is oriented."@en  ;
  rdfs:domain epos:Equipment ;
  rdfs:range rdfs:Literal ;
.

#extending range of schema:owns
#schema:owns
