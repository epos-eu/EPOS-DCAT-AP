  sh:property [
      sh:path schema:datePublished;
      sh:maxCount 1;
      sh:node epos:DateOrDateTimeDataType ;
    ] ;
  sh:property [
      sh:path schema:dateModified ;
      sh:minCount 1 ;
      sh:node epos:DateOrDateTimeDataType ;
      sh:message "Modified date is recommended. Please fill in a value"@en  ;
      sh:severity sh:Warning ;
    ] ;
