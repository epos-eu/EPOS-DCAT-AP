<http://www.epos-eu.org/epos/dcat-ap>
  rdf:type owl:Ontology ;
  dct:abstract "The EPOS DCAT Application Profile is an extension of the DCAT-AP for the EPOS community (www.epos-eu.org). It uses SHACL to express constraints and define the data structure (https://www.w3.org/TR/2017/REC-shacl-20170720/)"@en  ;
  dct:creator [
      foaf:name "Luca Trani" ;
    ] ;
  dct:contributor [
  	foaf:name "Rossana Paciello" ;
    ] ;
  dct:contributor [
  	foaf:name "Manuela Sbarra" ;
    ] ;
  dct:contributor [
  	foaf:name "Damian Ulbricht" ;
    ] ;
  dct:contributor [
  	foaf:name "Sylvain Grellet" ;
    ] ;
  dct:contributor [
  	foaf:name "Andy Riddick" ;
    ] ;
  dct:contributor [
  	foaf:name "Xiaoliang Wang" ;
    ] ;
  dct:created "2018-02-09"^^xsd:date ;
  dct:relation <https://joinup.ec.europa.eu/node/69559> ;
  dct:title "EPOS DCAT Application Profile"@en  ;
  rdfs:comment "This document specifies the set of RDF classes, properties and shapes used in the EPOS DCAT Application Profile."@en  ;
  rdfs:label "EPOS DCAT Application Profile for Data Portals"@en  ;
  owl:imports <http://datashapes.org/dash> ;
  owl:imports <http://www.schema.org> ;
  owl:imports <http://www.w3.org/2004/02/skos/core> ;
  owl:imports <http://www.w3.org/2006/vcard/ns> ;
  owl:imports <http://www.w3.org/2008/05/skos-xl> ;
  owl:imports <http://www.w3.org/ns/dcat> ;
  owl:imports <http://www.w3.org/ns/oa#> ;
  owl:imports org: ;
  owl:imports foaf: ;
  owl:versionInfo "0.9" ;
  foaf:homepage <http://www.epos-eu.org/dcat-ap> ;
.
  ####
  # Classes
  ###

epos:Equipment
  a owl:Class ;
  rdfs:comment "A resource representing a generic equipment. E.g. gps sensor, seismic station's channel"@en  ;
  rdfs:isDefinedBy <http://www.epos-eu.org/epos/dcat-ap> ;
  rdfs:label "Equipment"@en  ;
  rdfs:subClassOf rdfs:Resource ;
  rdfs:subClassOf schema:Product ;
.

epos:Facility
  a owl:Class ;
  rdfs:comment "A resource representing a Facility. E.g. seismic station"@en  ;
  rdfs:isDefinedBy <http://www.epos-eu.org/epos/dcat-ap> ;
  rdfs:label "Facility"@en  ;
  rdfs:subClassOf rdfs:Resource ;
.

##redefining schema:Organization and schema:Person
schema:Organization a owl:Class;
  owl:equivalentClass foaf:Organization;
  rdfs:label "Organization"@en;
.

schema:Person a owl:Class;
  owl:equivalentClass foaf:Person;
  rdfs:label "Person"@en;
.

#aligning vcard and schema
schema:ContactPoint a owl:Class;
    owl:equivalentClass vcard:Kind;
    rdfs:label "ContactPoint"@en;
.

epos:Publication
  a owl:Class ;
  rdfs:comment "EPOS specialisation of schema:CreativeWork"@en  ;
  rdfs:isDefinedBy <http://www.epos-eu.org/epos/dcat-ap> ;
  rdfs:label "Publication"@en  ;
  rdfs:subClassOf schema:CreativeWork ;
.

epos:WebService
  a owl:Class ;
  rdfs:comment "It represents a WebService API "@en  ;
  rdfs:subClassOf schema:Service;
  rdfs:subClassOf rdfs:Resource ;
  rdfs:label "WebService"@en  ;
  rdfs:isDefinedBy <http://www.epos-eu.org/epos/dcat-ap> ;
.

####
# Properties
####

epos:resource
  rdf:type owl:ObjectProperty ;
  rdfs:domain dcat:Catalog ;
  rdfs:range rdfs:Resource;
.

##We declare the equivalence of properties of dcat and schema.org
dcat:contactPoint owl:equivalentProperty schema:contactPoint .
dcat:keyword owl:equivalentProperty schema:keywords .

##We extend range and domain of dcat:contactPoint
dcat:contactPoint
  rdf:type owl:ObjectProperty ;
  rdfs:domain dcat:Dataset ;  
  rdfs:domain schema:Organization ;
  rdfs:domain schema:Person ;
  rdfs:domain epos:Equipment ;
  rdfs:domain schema:SoftwareApplication ;
  rdfs:domain schema:SoftwareSourceCode;
  rdfs:domain epos:WebService ;
  rdfs:domain foaf:Project ;
  rdfs:domain epos:Facility ;
  rdfs:range vcard:Kind;
  rdfs:range schema:ContactPoint ;
 .

epos:associatedProject
  rdf:type owl:ObjectProperty;
  rdfs:domain schema:Organization ;
  rdfs:range foaf:Project ;
.

epos:annotation
  rdf:type owl:ObjectProperty ;
  rdfs:domain rdfs:Resource ;
  rdfs:domain foaf:Agent ;
  rdfs:range oa:Annotation ;
.

dcat:theme
  rdf:type owl:ObjectProperty ;
  rdfs:comment "The main category/domain of the referred resource. It is an extension of dcat:theme"@en  ;
  rdfs:domain rdfs:Resource ;
  rdfs:domain dcat:Dataset ;
  rdfs:range skos:Concept ;
.

epos:dynamicRange
  rdf:type owl:DatatypeProperty ;
  rdfs:comment "This property contains the full scale measuring ability, in nT (unit and value)"@en  ;
  rdfs:domain epos:Equipment ;
  rdfs:range rdfs:Literal ;
.
epos:filter
  rdf:type owl:DatatypeProperty ;
  rdfs:comment "This property describes the filter that the instrument uses to produce data"@en  ;
  rdfs:domain epos:Equipment ;
  rdfs:range rdfs:Literal ;
.
epos:samplePeriod
  rdf:type owl:DatatypeProperty ;
  rdfs:comment "This property contains the sample period in ms"@en  ;
  rdfs:domain epos:Equipment ;
  rdfs:range rdfs:Literal ;
.

epos:orientation
  rdf:type owl:DatatypeProperty ;
  rdfs:comment "This property describes how the instrument is oriented."@en  ;
  rdfs:domain epos:Equipment ;
  rdfs:range rdfs:Literal ;
.

#extending range of schema:owns
schema:owns
 rdf:type owl:ObjectProperty ;
  rdfs:domain schema:Person ;
  rdfs:domain schema:Organization;
  rdfs:range schema:OwnershipInfo ;
  rdfs:range schema:Product ;
  rdfs:range epos:Facility;
.

epos:resolution
  rdf:type owl:DatatypeProperty ;
  rdfs:comment "This property contains the resolution in nT"@en  ;
  rdfs:domain epos:Equipment ;
  rdfs:range rdfs:Literal ;
.

####
# Shapes
####

epos:DateOrDateTimeDataType
  a sh:NodeShape ;
  sh:description "Date time date disjunction shape checks that a datatype property receives a date or a dateTime literal"@en  ;
  rdfs:label "Date time date disjunction"@en  ;
  sh:message "The values must be data typed as either xsd:date or xsd:dateTime"@en  ;
  sh:or (
      [
        sh:datatype xsd:date ;
      ]
      [
        sh:datatype xsd:dateTime ;
      ]
    ) ;
.

epos:ContactPointType
  a sh:NodeShape ;
  sh:description "A vcard:Kind or schema:ContactPoint"@en  ;
  sh:message "The values must be data either vcard:Kind or       schema:ContactPoint"@en  ;
  sh:or (
      [
        sh:class vcard:Kind ;
      ]
      [
        sh:class schema:ContactPoint ;
      ]
    ) ;
.


epos:CatalogShape
  a sh:NodeShape ;
  sh:targetClass dcat:Catalog ;

####
# Catalog Mandatory properties
###
  sh:property [
      sh:path dct:title ;
      sh:datatype xsd:string;
      sh:minCount 1 ;
    ] ;

  sh:property [
      sh:path dct:description ;
	sh:datatype xsd:string;
      sh:minCount 1 ;
    ] ;

  sh:property [
      sh:path dct:publisher ;
      sh:maxCount 1 ;
      sh:minCount 1 ;
      sh:or (
      [
        sh:class foaf:Agent ;
      ]
      [
        sh:class schema:Organization ;
      ]
    ) ;

    ] ;

  sh:property [
      sh:path dcat:dataset ;
      sh:class dcat:Dataset ;
      sh:minCount 1 ;
    ] ;

####
# Catalog Recommended properties
####

  sh:property [
      sh:path dct:issued ;
      sh:minCount 1 ;
      sh:message "Release date is recommended. Please fill in a value"@en  ;
      sh:node epos:DateOrDateTimeDataType ;
      sh:severity sh:Warning ;
    ] ;

  sh:property [
      sh:path dct:issued ;
      sh:maxCount 1 ;
      sh:node epos:DateOrDateTimeDataType ;
    ] ;

  sh:property [
      sh:path dct:modified ;
      sh:minCount 1 ;
      sh:message "Update/modification date is recommended. Please fill in a value"@en  ;
      sh:node epos:DateOrDateTimeDataType ;
      sh:severity sh:Warning ;
    ] ;

  sh:property [
      sh:path dct:modified ;
      sh:maxCount 1 ;
      sh:node epos:DateOrDateTimeDataType ;
    ] ;

  sh:property [
      sh:path dcat:themeTaxonomy ;
      sh:minCount 1 ;
      sh:message "Themes is recommended. Please fill in a value"@en  ;
      sh:datatype xsd:anyURI;
      sh:severity sh:Warning ;
    ] ;

  sh:property [
      sh:path foaf:homepage ;
      sh:minCount 1 ;
      sh:class foaf:Document ;
      sh:message "Homepage is recommended. Please fill in a value"@en  ;
      sh:severity sh:Warning ;
    ] ;

  sh:property [
      sh:path foaf:homepage ;
      sh:maxCount 1 ;
      sh:class foaf:Document ;
    ] ;

  sh:property [
      sh:path dct:license ;
      sh:minCount 1 ;
      sh:class dct:LicenseDocument ;
      sh:message "License is recommended. Please fill in a value"@en  ;
      sh:severity sh:Warning ;
    ] ;

  sh:property [
      sh:path dct:license ;
      sh:maxCount 1 ;
      sh:class dct:LicenseDocument ;
    ] ;

  sh:property [
      sh:path dct:language ;
      sh:minCount 1 ;
      sh:class dct:LinguisticSystem ;
      sh:message "Language is recommended. Please fill in a value"@en  ;
      sh:severity sh:Warning ;
    ] ;

####
# Catalog Optional properties
####

  sh:property [
      sh:path dct:hasPart ;
      sh:class dcat:Catalog ;
    ] ;

  sh:property [
      sh:path dct:isPartOf ;
