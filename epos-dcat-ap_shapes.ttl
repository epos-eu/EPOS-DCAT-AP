      sh:path schema:contactType ;
      sh:datatype xsd:string;
      sh:minCount 1 ;
      sh:message "contactType is recommended. Please fill in a value (e.g. legalContact, financialContact, scientificContact, manager)"@en  ;
      sh:severity sh:Warning ;
      #sh:in ( "legalContact" "financialContact" "scientificContact" "manager" );
    ] ;
    sh:property [
      sh:path schema:contactType ;
      sh:datatype xsd:string;
      sh:maxCount 1 ;
  ] ;
