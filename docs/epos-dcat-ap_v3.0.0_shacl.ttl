<http://www.epos-eu.org/epos-dcat-ap>
  rdf:type owl:Ontology ;
  dct:abstract "EPOS DCAT Application Profile is an extension of DCAT-AP for solid-Earth sciences communities."@en  ;
  dct:creator [foaf:name "Rossana Paciello" ; ] ;
  dct:creator [foaf:name "Manuela Sbarra" ;  ] ;
  dct:contributor [foaf:name "Valerio Vinciarelli" ; ] ;
  dct:created "2022-01-09"^^xsd:date ;
  dct:modified "2022-01-10"^^xsd:date ;
  dct:relation <https://joinup.ec.europa.eu/collection/semantic-interoperability-community-semic/solution/dcat-application-profile-data-portals-europe/release/210> ;
  dct:title "EPOS DCAT Application Profile"@en  ;
  rdfs:comment "This document specifies the set of classes, properties and shapes graphs used in the EPOS DCAT Application Profile."@en  ;
  rdfs:label "EPOS-DCAT-Application Profile"@en  ;
  owl:imports <http://datashapes.org/dash> ;
  #owl:imports <http://www.schema.org> ;
  #owl:imports <http://www.w3.org/2004/02/skos/core> ;
  #owl:imports <http://www.w3.org/2006/vcard/ns> ;
  #owl:imports <http://www.w3.org/2008/05/skos-xl> ;
  #owl:imports <http://www.w3.org/ns/dcat> ;
  #owl:imports <http://www.w3.org/ns/oa#> ;
  #owl:imports <http://www.w3.org/ns/org#> ;
  #owl:imports <http://xmlns.com/foaf/0.1/> ;
  owl:versionInfo "2.0" ;
  foaf:homepage <http://www.epos-eu.org/epos-dcat-ap> ; .

####
# Additional EPOS-DCAT-AP Classes
####
  epos:Equipment
    a owl:Class ;
    rdfs:comment "A generic equipment. E.g. a GPS sensor, a seismic station's channel"@en  ;
    rdfs:isDefinedBy <http://www.epos-eu.org/epos-dcat-ap> ;
    rdfs:label "Equipment"@en  ;
    rdfs:subClassOf rdfs:Resource ;
  .
  epos:Facility
    a owl:Class ;
    rdfs:comment "A resource representing a Facility. E.g. a laboratory, a seismic station"@en  ;
    rdfs:isDefinedBy <http://www.epos-eu.org/epos-dcat-ap> ;
    rdfs:label "Facility"@en  ;
    rdfs:subClassOf rdfs:Resource ;
  .
  epos:Publication
    a owl:Class ;
    rdfs:comment "A text publication is a resource relating to a dataset, data product or other. It is a subclass of schema:CreativeWork"@en  ;
    rdfs:isDefinedBy <http://www.epos-eu.org/epos-dcat-ap> ;
    rdfs:label "Publication"@en  ;
    rdfs:subClassOf schema:CreativeWork ;
  .

####
# Equivalent classes/properties for EPOS-DCAT-AP
####
# Classes ####
  ##Alignment schema:ContactPoint and vcard:Kind##
  schema:ContactPoint a owl:Class;
    owl:equivalentClass vcard:Kind;
    rdfs:label "ContactPoint"@en;
  .
  ##Alignment schema:Organization and foaf:Organization##
  schema:Organization a owl:Class;
    owl:equivalentClass foaf:Organization;
    rdfs:label "Organization"@en;
  .
  ##Alignment schema:Person and foaf:Person##
  schema:Person a owl:Class;
    owl:equivalentClass foaf:Person;
    rdfs:label "Person"@en;
  .
# Properties ####
  ##Alignment dcat:contactPoint and schema:contactPoint##
  dcat:contactPoint owl:equivalentProperty schema:contactPoint .

  ##Alignment dcat:keyword and schema:keywords##
  dcat:keyword owl:equivalentProperty schema:keywords .

####
# Extending range and domain for EPOS-DCAT-AP
####
# Extending range and domain of dcat:contactPoint##
  dcat:contactPoint
    rdf:type owl:ObjectProperty ;
    rdfs:domain [ rdf:type owl:Class ;
    	owl:unionOf (dcat:Dataset schema:Organization schema:Person epos:Equipment schema:SoftwareApplication schema:SoftwareSourceCode dcat:DataService foaf:Project epos:Facility)];
    rdfs:range [ rdf:type owl:Class ;
    	owl:unionOf (vcard:Kind schema:ContactPoint)];
  .
# Extending the domain of dcat:theme##
  dcat:theme
    rdf:type owl:ObjectProperty ;
    rdfs:comment "The main category/domain of the referred resource."@en  ;
    rdfs:domain [rdf:type owl:Class ;
    	owl:unionOf (rdfs:Resource dcat:Dataset)] ;
    rdfs:range skos:Concept ;
  .
# Extending the range of schema:owns##
  schema:owns
    rdf:type owl:ObjectProperty ;
    rdfs:domain [rdf:type owl:Class ;
    	owl:unionOf (schema:Person schema:Organization)];
    rdfs:range [rdf:type owl:Class ;
    	owl:unionOf (schema:OwnershipInfo schema:Product epos:Facility)];
  .

####
# EPOS Properties definition
####
# Organization properties
  epos:associatedProject
    rdf:type owl:ObjectProperty;
    rdfs:comment "This property refers to a Project in which the Organization is involved."@en  ;
    rdfs:domain schema:Organization ;
    rdfs:range foaf:Project ;
  .
# Equipment properties
  epos:dynamicRange
    rdf:type owl:ObjectProperty ;
    rdfs:comment "This property contains the full scale measuring ability, in nT (unit and value)"@en  ;
    rdfs:domain epos:Equipment ;
    rdfs:range rdfs:Literal ;
  .
  epos:filter
    rdf:type owl:ObjectProperty ;
    rdfs:comment "This property describes the filter that an instrument might apply to produce data"@en  ;
    rdfs:domain epos:Equipment ;
    rdfs:range rdfs:Literal ;
  .
  epos:orientation
    rdf:type owl:ObjectProperty ;
    rdfs:comment "This property describes how the instrument is oriented."@en  ;
    rdfs:domain epos:Equipment ;
    rdfs:range rdfs:Literal ;
  .
  epos:resolution
    rdf:type owl:ObjectProperty ;
    rdfs:comment "This property contains the resolution"@en  ;
    rdfs:domain epos:Equipment ;
    rdfs:range rdfs:Literal ;
  .
  epos:samplePeriod
    rdf:type owl:ObjectProperty ;
    rdfs:comment "This property contains the sample period"@en  ;
    rdfs:domain epos:Equipment ;
    rdfs:range rdfs:Literal ;
  .
# Spatial properties##
  epos:northernmostLatitude
    rdf:type owl:DatatypeProperty ;
    rdfs:subPropertyOf geo:lat;
    rdfs:comment "The WGS84 upper bound (max) latitude of a SpatialThing (decimal degrees)"
  .
  epos:southernmostLatitude
    rdf:type owl:DatatypeProperty ;
    rdfs:subPropertyOf geo:lat;
    rdfs:comment "The WGS84 lower bound (min) latitude of a SpatialThing (decimal degrees)"
  .
  epos:westernmostLongitude
    rdf:type owl:DatatypeProperty ;
    rdfs:subPropertyOf geo:long;
    rdfs:comment "The WGS84 lower bound (min) longitude of a SpatialThing (decimal degrees)"
  .
  epos:easternmostLongitude
    rdf:type owl:DatatypeProperty ;
    rdfs:subPropertyOf geo:long;
    rdfs:comment "The WGS84 upper bound (max) longitude of a SpatialThing (decimal degrees)"
  .

####
# SHACL Shapes Graphs
####
epos:DcatResourceShape
  a sh:NodeShape ;
  sh:description "The union of Catalog, Dataset and DataService" ;
  sh:message "The node is either a Catalog, Dataset or a DataService" ;
  sh:or ([ sh:class dcat:Catalog ]
         [ sh:class dcat:Dataset ]
         [ sh:class dcat:DataService ]
  )
.
epos:DateOrDateTimeDataType
  a sh:NodeShape ;
  sh:description "It checks that a datatype property receives a date or a dateTime literal"@en  ;
  sh:message "The values must be data typed as either xsd:date, xsd:dateTime, xsd:gYear or xsd:gYearMonth"@en  ;
  sh:or ( [ sh:datatype xsd:date ; ]
          [ sh:datatype xsd:dateTime ; ]
	        [ sh:datatype xsd:gYear ; ]
          [ sh:datatype xsd:gYearMonth ; ]
 ) ;
.
epos:ContactPointType
  a sh:NodeShape ;
  sh:description "A vcard:Kind or schema:ContactPoint"@en  ;
  sh:message "The values must be data either vcard:Kind or schema:ContactPoint"@en  ;
  sh:or ( [sh:class vcard:Kind ; ]
      [ sh:class schema:ContactPoint ; ]) ;
.
epos:CatalogShape
    a sh:NodeShape ;
    sh:targetClass dcat:Catalog ;
  ####
  # Catalog mandatory properties
  ####
  sh:property [
      sh:path dct:description ;
      sh:description "This property contains a free-text account of the Catalog. This property can be repeated for parallel language versions of the description.";
      sh:datatype xsd:string;
      sh:minCount 1 ;
    ] ;
  sh:property [
      sh:path dct:publisher ;
      sh:description "This property refers to an entity (organisation) responsible for making the Catalog available.";
      sh:minCount 1 ;
      sh:maxCount 1 ;
      sh:or (
        [sh:class foaf:Agent ; ]
        [sh:class schema:Organization ; ]) ;
    ] ;
  sh:property [
      sh:path dct:title ;
      sh:description "This property contains a name given to the Catalog. This property can be repeated for parallel language versions of the name." ;
      sh:datatype xsd:string;
      sh:minCount 1 ;
    ] ;
  ####
  # Catalog recommended properties
  ####
  sh:property [
      sh:path dcat:dataset ;
      sh:description "This property links the Catalog with a Dataset that is part of the Catalog.";
      sh:class dcat:Dataset ;
      sh:minCount 1 ;
      sh:message "Dataset is recommended. Please fill in a value"@en  ;
      sh:severity sh:Warning ;
    ] ;
  sh:property [
      sh:path dct:issued ;
      sh:node epos:DateOrDateTimeDataType ;
      sh:minCount 1 ;
      sh:message "Issued date is recommended. Please fill in a value"@en  ;
      sh:severity sh:Warning ;
    ] ;
  sh:property [
      sh:path dct:issued ;
      sh:description "This property contains the date of formal issuance (e.g., publication) of the Catalog.";
      sh:node epos:DateOrDateTimeDataType ;
      sh:maxCount 1 ;
    ] ;
  sh:property [
      sh:path foaf:homepage ;
      sh:class foaf:Document ;
      sh:minCount 1 ;
      sh:message "Homepage is recommended. Please fill in a value"@en  ;
      sh:severity sh:Warning ;
    ] ;
  sh:property [
      sh:path foaf:homepage ;
      sh:description "This property refers to a web page that acts as the main page for the Catalog.";
      sh:class foaf:Document ;
      sh:maxCount 1 ;
    ] ;
  sh:property [
      sh:path dct:language ;
      sh:description "This property refers to a language used in the textual metadata describing titles, descriptions, etc. of the Datasets in the Catalog. This property can be repeated if the metadata is provided in multiple languages.";
      sh:minCount 1 ;
      sh:class dct:LinguisticSystem ;
      sh:message "Language is recommended. Please fill in a value"@en  ;
      sh:severity sh:Warning ;
    ] ;
  sh:property [
      sh:path dct:license ;
      sh:class dct:LicenseDocument ;
      sh:minCount 1 ;
      sh:message "License is recommended. Please fill in a value"@en  ;
      sh:severity sh:Warning ;
    ] ;
  sh:property [
      sh:path dct:license ;
      sh:description "This property refers to the licence under which the Catalog can be used or reused.";
      sh:class dct:LicenseDocument ;
      sh:maxCount 1 ;
    ] ;
  sh:property [
      sh:path dct:modified ;
      sh:node epos:DateOrDateTimeDataType ;
      sh:minCount 1 ;
      sh:message "Update/modification date is recommended. Please fill in a value"@en  ;
      sh:severity sh:Warning ;
    ] ;
  sh:property [
      sh:path dct:modified ;
      sh:description "This property contains the most recent date on which the Catalog was modified.";
      sh:node epos:DateOrDateTimeDataType ;
      sh:maxCount 1 ;
    ] ;
  sh:property [
      sh:path dct:spatial ;
      sh:class dct:Location ;
      sh:description "This property refers to a geographical area covered by the Catalog.";
      sh:minCount 1 ;
      sh:severity sh:Warning ;
    ] ;
  sh:property [
      sh:path dcat:service ;
      sh:description "This property refers to a site or end-point (Data Service) that is listed in the Catalog. As empty Catalog are usually indications of problems, this property should be combined with the previous property dataset to implement an empty Catalog check.";
      sh:class dcat:DataService ;
      sh:minCount 1 ;
      sh:message "Service is recommended. Please fill in a value"@en  ;
      sh:severity sh:Warning ;
  ] ;
  sh:property [
      sh:path dcat:themeTaxonomy ;
      sh:description "This property refers to a knowledge organization system used to classify the resources documented in the Catalog (Data Services, Datasets, and other Catalogs).";
      sh:class skos:ConceptScheme ;
      sh:minCount 1 ;
      sh:message "themeTaxonomy is recommended. Please fill in a value"@en  ;
      sh:severity sh:Warning ;
    ] ;
  ####
  # Catalog optional properties
  ####
  sh:property [
      sh:path dcat:catalog;
      sh:description "This property refers to a catalog whose contents are of interest in the context of this catalog ";
      sh:class dcat:Catalog ;
    ] ;
  sh:property [
      sh:path dct:creator;
      sh:description "This property refers to the Agent primarily responsible for producing the catalog. ";
      sh:class foaf:Agent;
      sh:maxCount 1 ;
    ] ;
  sh:property [
      sh:path dct:hasPart ;
      sh:description "This property refers to a related Catalog that is part of the described Catalog";
      sh:class dcat:Catalog ;
    ] ;
  sh:property [
      sh:path dct:isPartOf ;
      sh:description "This property refers to a related Catalog in which the described Catalog is physically or logically included.";
      sh:class dcat:Catalog ;
      sh:maxCount 1 ;
    ] ;
  sh:property [
      sh:path dcat:record ;
      sh:description "This property refers to a Catalog Record that is part of the Catalog";
      sh:class dcat:CatalogRecord ;
    ] ;
  sh:property [
      sh:path dct:rights ;
      sh:description "This property refers to a statement that specifies rights associated with the Catalog.";
      sh:class dct:RightsStatement ;
      sh:maxCount 1 ;
    ] ;
  sh:property [
      sh:path dct:temporal ;
      sh:description "This property refers to a temporal period that the Catalog covers.";
      sh:class dct:PeriodOfTime ;
    ] ;
.
epos:CatalogRecordShape
  a sh:NodeShape ;
  sh:targetClass dcat:CatalogRecord;
  ####
  # CatalogRecord mandatory properties
  ####
  sh:property [
      sh:path dct:modified ;
      sh:description "This property contains the most recent date on which the Catalog Record was modified.";
      sh:node epos:DateOrDateTimeDataType ;
      sh:maxCount 1 ;
      sh:minCount 1 ;
    ] ;
  sh:property [
      sh:path foaf:primaryTopic ;
      sh:description "This property links the Catalog Record to the Dataset described in the record.";
      sh:node epos:DcatResourceShape ;
      sh:maxCount 1 ;
      sh:minCount 1 ;
    ] ;
  ####
  # CatalogRecord recommended properties
  ####
  sh:property [
      sh:path dct:conformsTo ;
      sh:datatype xsd:anyURI;
      sh:minCount 1 ;
      sh:message "ConformsTo is recommended. Please fill in a value"@en  ;
      sh:severity sh:Warning ;
    ] ;
  sh:property [
      sh:path dct:conformsTo ;
      sh:description "This property refers to an Application Profile that the Dataset’s metadata conforms to";
      sh:datatype xsd:anyURI;
      sh:maxCount 1 ;
    ] ;
  sh:property [
      sh:path dct:identifier ;
      sh:minCount 1 ;
      sh:or ( [sh:datatype xsd:string;]
        [sh:datatype xsd:anyURI; ] );
      sh:message "Identifier is recommended. Please fill in a value"@en  ;
      sh:severity sh:Warning ;
    ] ;
  sh:property [
      sh:path dct:identifier;
      sh:description "This property contains the main identifier for the Catalog Record.";
      sh:or ( [sh:datatype xsd:string; ]
        [sh:datatype xsd:anyURI; ] );
      sh:maxCount 1 ;
    ] ;
  sh:property [
      sh:path dct:issued ;
      sh:minCount 1 ;
      sh:node epos:DateOrDateTimeDataType ;
      sh:message "Issued is recommended. Please fill in a value"@en  ;
      sh:severity sh:Warning ;
    ] ;
  sh:property [
      sh:path dct:issued ;
      sh:description "This property contains the date on which the description of the Dataset was included in the Catalog.";
      sh:node epos:DateOrDateTimeDataType ;
      sh:maxCount 1 ;
    ] ;
  sh:property [
      sh:path adms:status ;
      sh:datatype xsd:anyURI;
      sh:minCount 1 ;
      sh:message "Status is recommended. Please fill in a value"@en  ;
      sh:severity sh:Warning ;
    ] ;
  sh:property [
      sh:path adms:status ;
      sh:datatype xsd:anyURI;
      sh:description "This property refers to the type of the latest revision of Catalog Record.";
      sh:maxCount 1 ;
    ] ;
  ####
  # CatalogRecord optional properties
  ####
  sh:property [
      sh:path dct:description ;
      sh:description "This property contains a free-text account of the record. This property can be repeated for parallel language versions of the description.";
      sh:datatype xsd:string;
    ] ;
  sh:property [
      sh:path dct:language ;
      sh:description "This property refers to a language used in the textual metadata describing titles, descriptions, etc. of the Catalog Record. This property can be repeated if the metadata is provided in multiple languages.";
      sh:class dct:LinguisticSystem ;
    ] ;
  sh:property [
      sh:path dct:source ;
      sh:description "This property refers to the original metadata that was used in creating the Catalog Record. ";
      sh:class dcat:CatalogRecord ;
      sh:maxCount 1 ;
    ] ;
  sh:property [
      sh:path dct:title ;
      sh:description "This property contains a name given to the Catalog Record. This property can be repeated for parallel language versions of the name.";
      sh:datatype xsd:string;
    ] ;
.
epos:DataServiceShape
  a sh:NodeShape ;
  sh:targetClass dcat:DataService;
  ####
  # DataService mandatory properties
  ####
  sh:property [
      sh:path dcat:endpointURL ;
      sh:description " The root location or primary endpoint of the service (an IRI).";
      sh:or ( [ sh:class rdfs:Resource ; ]
              [ sh:datatype xsd:anyURI ] ) ;
      sh:minCount 1 ;
    ] ;
  sh:property [
      sh:path dct:identifier ;
      sh:description "This property contains the main identifier for the Data Service.";
      sh:or ( [sh:datatype xsd:string; ]
              [ sh:datatype xsd:anyURI; ] ) ;
      sh:minCount 1 ;
      sh:maxCount 1 ;
    ] ;
  sh:property [
      sh:path dct:title ;
      sh:description " This property contains a name given to the Data Service. This property can be repeated for parallel language versions of the name. ";                 sh:minCount 1 ;
      sh:datatype xsd:string;
      sh:minCount 1 ;
    ] ;
  ####
  # DataService recommended properties
  ####
  sh:property [
      sh:path dcat:contactPoint ;
      sh:description "This property contains contact information that can be used for sending comments about the Data Service.";
      sh:node epos:ContactPointType ;
      sh:minCount 1;
      sh:message "Contact Point is recommended. Please fill in a value"@en  ;
      sh:severity sh:Warning ;
    ] ;
  sh:property [
      sh:path dct:description ;
      sh:datatype xsd:string;
      sh:minCount 1 ;
      sh:description " This property contains a free-text account of the Data Service. This property can be repeated for parallel language versions of the description.";
      sh:message "Description is recommended. Please fill in a value"@en  ;
      sh:severity sh:Warning ;
    ] ;
  sh:property [
      sh:path dcat:endpointDescription ;
      sh:description "This property contains a description of the services available via the end-points, including their operations, parameters etc. The property gives specific details of the actual endpoint instances, while dct:conformsTo is used to indicate the general standard or specification that the endpoints implement. ";
      sh:or ( [  sh:class rdfs:Resource ; ]
            [  sh:class hydra:Operation ; ] );
      sh:minCount 1 ;
      sh:message "endpointDescription is recommended. Please fill in a value"@en  ;
      sh:severity sh:Warning ;
    ] ;
  sh:property [
      sh:path dct:issued;
      sh:node epos:DateOrDateTimeDataType ;
      sh:minCount 1 ;
      sh:message "Issued is recommended. Please fill in a value"@en  ;
      sh:severity sh:Warning ;
    ] ;
  sh:property [
      sh:path dct:issued;
      sh:description " This property contains the date of publication of the Data Service.";
      sh:node epos:DateOrDateTimeDataType ;
      sh:maxCount 1;
    ] ;
  sh:property [
      sh:path dct:modified ;
      sh:node epos:DateOrDateTimeDataType ;
      sh:minCount 1 ;
      sh:message "Modified date is recommended. Please fill in a value"@en  ;
      sh:severity sh:Warning ;
    ] ;
  sh:property [
      sh:path dct:modified ;
      sh:description " This property contains the most recent date on which the Data Service was modified.";
      sh:node epos:DateOrDateTimeDataType ;
      sh:maxCount 1;
    ] ;
  sh:property [
      sh:path schema:provider ;
      sh:class schema:Organization ;
      sh:minCount 1 ;
      sh:message "Provider is recommended. Please fill in a value"@en  ;
      sh:severity sh:Warning ;
    ] ;
  sh:property [
      sh:path schema:provider ;
      sh:description " This property refers to an Organisation responsible for making the Data Service available.";
      sh:class schema:Organization ;
      sh:maxCount 1;
    ] ;
  sh:property [
      sh:path dcat:servesDataset ;
      sh:description " This property refers to a collection of data that this data service can distribute.";
      sh:class dcat:Dataset;
      sh:minCount 1 ;
      sh:message " servesDataset is recommended. Please fill in a value"@en  ;
      sh:severity sh:Warning ;
    ] ;
  sh:property [
      sh:path dcat:theme;
      sh:description "This property refers to a category of the Data Service. A Data Service may be associated with multiple themes.";
      sh:class skos:Concept ;
      sh:minCount 1 ;
      sh:message "Theme is recommended. Please fill in a value"@en  ;
      sh:severity sh:Warning ;
    ] ;
  ####
  # DataService optional properties
  ####
  sh:property [
      sh:path dct:accessRight ;
      sh:description " This property MAY include information regarding access or restrictions based on privacy, security, or other policies.";
      sh:class dct:RightsStatement;
      sh:maxCount 1;
    ] ;
  sh:property [
      sh:path dct:conformsTo ;
      sh:description " This property indicates the general standard or specification that the endpoints implement. E.g. http://www.opengis.net/def/serviceType/ogc/wfs/1.0.0.";
      sh:or ( [ sh:class dct:Standard ; ]
              [ sh:datatype xsd:anyURI ; ] );
    ] ;
  sh:property [
      sh:path dct:format;
      sh:description " This property refers to the structure that can be returned by querying the endpointURL. ";
      sh:or ( [ sh:datatype xsd:string ; ]
              [ sh:datatype xsd:anyURI ; ] );
   ] ;
  sh:property [
      sh:path dcat:keyword;
      sh:description " This property contains a keyword or tag describing the Data Service. ";
      sh:datatype xsd:string ;
    ] ;
  sh:property [
