@prefix locn: <http://www.w3.org/ns/locn#> .
@prefix gsp: <http://www.opengis.net/ont/geosparql#> .

<http://www.epos-eu.org/epos-dcat-ap>
  rdf:type owl:Ontology ;
  dct:abstract "EPOS DCAT Application Profile is an extension of DCAT-AP for solid-Earth sciences communities."@en  ;
  dct:creator [  foaf:name "Luca Trani" ; ] ;
  dct:contributor [foaf:name "Rossana Paciello" ; ] ;
  dct:contributor [foaf:name "Manuela Sbarra" ;  ] ;
  dct:contributor [foaf:name "Damian Ulbricht" ; ] ;
  dct:contributor [foaf:name "Sylvain Grellet" ;  ] ;
  dct:contributor [foaf:name "Andy Riddick" ;  ] ;
  dct:contributor [foaf:name "Xiaoliang Wang" ; ] ;
  dct:created "2018-02-09"^^xsd:date ;
  dct:modified "2019-05-21"^^xsd:date ;
  dct:relation <https://joinup.ec.europa.eu/node/69559> ;
  dct:title "EPOS DCAT Application Profile"@en  ;
  rdfs:comment "This document specifies the set of classes, properties and shapes graphs used in the EPOS DCAT Application Profile."@en  ;
  rdfs:label "EPOS-DCAT-Application Profile"@en  ;
  owl:imports <http://datashapes.org/dash> ;
 # owl:imports <http://www.schema.org> ;
 # owl:imports <http://www.w3.org/2004/02/skos/core> ;
  #owl:imports <http://www.w3.org/2006/vcard/ns> ;
  #owl:imports <http://www.w3.org/2008/05/skos-xl> ;
  #owl:imports <http://www.w3.org/ns/dcat> ;
 # owl:imports <http://www.w3.org/ns/oa#> ;
  #owl:imports <http://www.w3.org/ns/org#> ;
  #owl:imports <http://xmlns.com/foaf/0.1/> ;
  owl:versionInfo "0.15" ;
  foaf:homepage <http://www.epos-eu.org/epos-dcat-ap> ; .
####
# Classes
####
epos:Equipment
  a owl:Class ;
  rdfs:comment "A generic equipment. E.g. a GPS sensor, a seismic station's channel"@en  ;
  rdfs:isDefinedBy <http://www.epos-eu.org/epos-dcat-ap> ;
  rdfs:label "Equipment"@en  ;
  rdfs:subClassOf rdfs:Resource ;
  rdfs:subClassOf schema:Product ;
.
epos:Facility
  a owl:Class ;
  rdfs:comment "A resource representing a Facility. E.g. a laboratory, a seismic station"@en  ;
  rdfs:isDefinedBy <http://www.epos-eu.org/epos-dcat-ap> ;
  rdfs:label "Facility"@en  ;
  rdfs:subClassOf rdfs:Resource ;
.
epos:Publication
  a owl:Class ;
  rdfs:comment "EPOS specialisation of schema:CreativeWork"@en  ;
  rdfs:isDefinedBy <http://www.epos-eu.org/epos-dcat-ap> ;
  rdfs:label "Publication"@en  ;
  rdfs:subClassOf schema:CreativeWork ;
.
epos:WebService
  a owl:Class ;
  rdfs:comment "A service accessible via a Web API "@en  ;
  rdfs:subClassOf schema:Service;
  rdfs:subClassOf rdfs:Resource ;
  rdfs:label "WebService"@en  ;
  rdfs:isDefinedBy <http://www.epos-eu.org/epos-dcat-ap> ;
.
##Alignment schema:Organization and foaf:Organization##
schema:Organization a owl:Class;
  owl:equivalentClass foaf:Organization;
  rdfs:label "Organization"@en;
.
##Alignment schema:Person and foaf:Person##
schema:Person a owl:Class;
  owl:equivalentClass foaf:Person;
  rdfs:label "Person"@en;
.
##Alignment schema:ContactPoint and vcard:Kind##
schema:ContactPoint a owl:Class;
    owl:equivalentClass vcard:Kind;
    rdfs:label "ContactPoint"@en;
.
####
# Properties
####
epos:resource
  rdf:type owl:ObjectProperty ;
  rdfs:domain dcat:Catalog ;
  rdfs:range rdfs:Resource;
.
##Alignment dcat:contactPoint and schema:contactPoint##
dcat:contactPoint owl:equivalentProperty schema:contactPoint .
##Alignment dcat:keyword and schema:keywords##
dcat:keyword owl:equivalentProperty schema:keywords .
##Extending range and domain of dcat:contactPoint##
dcat:contactPoint
  rdf:type owl:ObjectProperty ;
  rdfs:domain [ rdf:type owl:Class ;
      owl:unionOf (dcat:Dataset schema:Organization schema:Person epos:Equipment schema:SoftwareApplication schema:SoftwareSourceCode
  epos:WebService foaf:Project epos:Facility)
  ];
  rdfs:range [ rdf:type owl:Class ;
      owl:unionOf (vcard:Kind schema:ContactPoint)
  ];
.
epos:associatedProject
  rdf:type owl:ObjectProperty;
  rdfs:domain schema:Organization ;
  rdfs:range foaf:Project ;
.
epos:annotation
  rdf:type owl:ObjectProperty ;
  rdfs:domain [rdf:type owl:Class ;
  owl:unionOf (rdfs:Resource foaf:Agent dcat:Distribution)] ;
  rdfs:range oa:Annotation ;
.
##Extending the domain of dcat:theme##
dcat:theme
  rdf:type owl:ObjectProperty ;
  rdfs:comment "The main category/domain of the referred resource."@en  ;
  rdfs:domain [rdf:type owl:Class ;
  owl:unionOf (rdfs:Resource dcat:Dataset)] ;
  rdfs:range skos:Concept ;
.
epos:dynamicRange
  rdf:type owl:ObjectProperty ;
  rdfs:comment "This property contains the full scale measuring ability, in nT (unit and value)"@en  ;
  rdfs:domain epos:Equipment ;
  rdfs:range schema:QuantitativeValue ;
.
epos:filter
  rdf:type owl:ObjectProperty ;
  rdfs:comment "This property describes the filter that an instrument might apply to produce data"@en  ;
  rdfs:domain epos:Equipment ;
  rdfs:range rdfs:Literal ;
.
epos:samplePeriod
  rdf:type owl:ObjectProperty ;
  rdfs:comment "This property contains the sample period in ms"@en  ;
  rdfs:domain epos:Equipment ;
  rdfs:range schema:QuantitativeValue ;
.
epos:orientation
  rdf:type owl:ObjectProperty ;
  rdfs:comment "This property describes how the instrument is oriented."@en  ;
  rdfs:domain epos:Equipment ;
  rdfs:range rdfs:Literal ;
.
##Spatial properties##
epos:northernmostLatitude
  rdf:type owl:DatatypeProperty ;
  rdfs:subPropertyOf geo:lat;
  rdfs:comment "The WGS84 upper bound (max) latitude of a SpatialThing (decimal degrees)"
.
epos:southernmostLatitude
  rdf:type owl:DatatypeProperty ;
  rdfs:subPropertyOf geo:lat;
  rdfs:comment "The WGS84 lower bound (min) latitude of a SpatialThing (decimal degrees)"
.
epos:westernmostLongitude
  rdf:type owl:DatatypeProperty ;
  rdfs:subPropertyOf geo:long;
  rdfs:comment "The WGS84 lower bound (min) longitude of a SpatialThing (decimal degrees)"
.
epos:easternmostLongitude
  rdf:type owl:DatatypeProperty ;
  rdfs:subPropertyOf geo:long;
  rdfs:comment "The WGS84 upper bound (max) longitude of a SpatialThing (decimal degrees)"
.
##Extending the range of schema:owns##
schema:owns
 rdf:type owl:ObjectProperty ;
  rdfs:domain [rdf:type owl:Class ;
  owl:unionOf (schema:Person schema:Organization)];
  rdfs:range [rdf:type owl:Class ;
  owl:unionOf (schema:OwnershipInfo schema:Product epos:Facility)];
.
epos:resolution
  rdf:type owl:ObjectProperty ;
  rdfs:comment "This property contains the resolution in nT"@en  ;
  rdfs:domain epos:Equipment ;
  rdfs:range rdfs:Literal ;
.
####
# SHACL Shapes Graphs
####
epos:DateOrDateTimeDataType
  a sh:PropertyShape ;
  sh:description "It checks that a datatype property receives a date or a dateTime literal"@en  ;
  sh:message "The values must be data typed as either xsd:date or xsd:dateTime"@en  ;
  sh:or ( [ sh:datatype xsd:date ; ]
             [ sh:datatype xsd:dateTime ; ] ) ;
.
epos:ContactPointType
  a sh:PropertyShape ;
  sh:description "A vcard:Kind or schema:ContactPoint"@en  ;
  sh:message "The values must be data either vcard:Kind or schema:ContactPoint"@en  ;
  sh:or ( [sh:class vcard:Kind ; ]
      [ sh:class schema:ContactPoint ; ]) ;
.
epos:CatalogShape
  a sh:NodeShape ;
  sh:targetClass dcat:Catalog ;
####sh:path dcat:contactPoint ;
# Catalog mandatory properties
####
  sh:property [
      sh:path dct:title ;
      sh:description "This property contains a name given to the Catalogue. This property can be repeated for parallel language versions of the name." ;
      sh:datatype xsd:string;
      sh:minCount 1 ;
  ] ;
  sh:property [
      sh:path dct:description ;
      sh:description "This property contains a free-text account of the Catalogue. This property can be repeated for parallel language versions of the description.";
      sh:datatype xsd:string;
      sh:minCount 1 ;
  ] ;
  sh:property [
      sh:path dct:publisher ;
      sh:description "This property refers to an entity (organisation) responsible for making the Catalogue available.";
      sh:maxCount 1 ;
      sh:minCount 1 ;
      sh:or (
        [sh:class foaf:Agent ; ]
        [sh:class schema:Organization ; ]) ;
  ] ;
  sh:property [
      sh:path dcat:dataset ;
      sh:description "This property links the Catalogue with a Dataset that is part of the Catalogue.";
      sh:class dcat:Dataset ;
      sh:minCount 1 ;
  ] ;
####
# Catalog recommended properties
####
  sh:property [
      sh:path dct:issued ;
      sh:minCount 1 ;
      sh:message "Release date is recommended. Please fill in a value"@en  ;
      sh:node epos:DateOrDateTimeDataType ;
      sh:severity sh:Warning ;
  ] ;
  sh:property [
      sh:path dct:issued ;
      sh:description "This property contains the date of formal issuance (e.g., publication) of the Catalogue.";
      sh:maxCount 1 ;
      sh:node epos:DateOrDateTimeDataType ;
  ] ;
  sh:property [
      sh:path dct:modified ;
      sh:minCount 1 ;
      sh:message "Update/modification date is recommended. Please fill in a value"@en  ;
      sh:node epos:DateOrDateTimeDataType ;
      sh:severity sh:Warning ;
  ] ;
  sh:property [
      sh:path dct:modified ;
      sh:description "This property contains the most recent date on which the Catalogue was modified.";
      sh:maxCount 1 ;
      sh:node epos:DateOrDateTimeDataType ;
  ] ;
  sh:property [
      sh:path dcat:themeTaxonomy ;
      sh:description "This property refers to a knowledge organization system used to classify the Catalogue's Datasets.";
      sh:minCount 1 ;
      sh:message "Theme is recommended. Please fill in a value"@en  ;
      sh:datatype xsd:anyURI;
      sh:severity sh:Warning ;
  ] ;
  sh:property [
      sh:path foaf:homepage ;
      sh:minCount 1 ;
      sh:class foaf:Document ;
      sh:message "Homepage is recommended. Please fill in a value"@en  ;
      sh:severity sh:Warning ;
  ] ;
  sh:property [
      sh:path foaf:homepage ;
      sh:description "This property refers to a web page that acts as the main page for the Catalogue.";
      sh:maxCount 1 ;
      sh:class foaf:Document ;
  ] ;
  sh:property [
      sh:path dct:license ;
      sh:minCount 1 ;
      sh:class dct:LicenseDocument ;
      sh:message "License is recommended. Please fill in a value"@en  ;
      sh:severity sh:Warning ;
  ] ;
  sh:property [
      sh:path dct:license ;
      sh:description "This property refers to the licence under which the Catalogue can be used or reused.";
      sh:maxCount 1 ;
      sh:class dct:LicenseDocument ;
  ] ;
  sh:property [
      sh:path dct:language ;
      sh:description "This property refers to a language used in the textual metadata describing titles, descriptions, etc. of the Datasets in the Catalogue. This property can be repeated if the metadata is provided in multiple languages.";
      sh:minCount 1 ;
      sh:class dct:LinguisticSystem sh:path dcat:contactPoint ;;
      sh:message "Language is recommended. Please fill in a value"@en  ;
      sh:severity sh:Warning ;
  ] ;
####
# Catalog optional properties
####
  sh:property [
      sh:path dct:hasPart ;
      sh:class dcat:Catalog ;
      sh:description "This property refers to a related Catalogue that is part of the described Catalogue";
  ] ;
  sh:property [
      sh:path dct:isPartOf ;
      sh:class dcat:Catalog ;
      sh:description "This property refers to a related Catalogue in which the described Catalogue is physically or logically included.";
      sh:maxCount 1 ;
  ] ;
  sh:property [
      sh:path dct:rights ;
      sh:class dct:RightsStatement ;
      sh:description "This property refers to a statement that specifies rights associated with the Catalogue.";
      sh:maxCount 1 ;
  ] ;
  sh:property [
      sh:path dct:spatial ;
      sh:class dct:Location ;
      sh:description "This property sh:path dcat:contactPoint ;refers to a geographical area covered by the Catalogue.";
  ] ;
  sh:property [
      sh:path dcat:record ;
      sh:class dcat:CatalogRecord ;
      sh:description "This property refers to a Catalogue Record that is part of the Catalogue";
  ] ;
  sh:property [
      sh:path epos:resource ;
      sh:class rdfs:Resource ;
      sh:description "This property links the Catalogue with a Resource.";
  ] ;
.
epos:CatalogRecordShape
  a sh:NodeShape ;
  sh:targetClass dcat:CatalogRecord;
####
# CatalogRecord mandatory properties
####
  sh:property [
      sh:path foaf:primaryTopic ;
      sh:class dcat:Dataset ;
      sh:description "This property links the Catalogue Record to the Dataset described in the record.";
      sh:maxCount 1 ;
      sh:minCount 1 ;
  ] ;
  sh:property [
      sh:path dct:modified ;
      sh:description "This property contains the most recent date on which the Catalog Record was modified.";
      sh:maxCount 1 ;
      sh:minCount 1 ;
      sh:node epos:DateOrDateTimeDataType ;
  ] ;
####
# CatalogRecord recommended properties
####
  sh:property [
      sh:path dct:identifier ;
      sh:minCount 1 ;
      sh:or ( [sh:datatype xsd:string;]
        [sh:datatype xsd:anyURI; ] );
      sh:message "Identifier is recommended. Please fill in a value"@en  ;
      sh:severity sh:Warning ;
  ] ;
  sh:property [
      sh:path dct:identifier;
      sh:or ( [sh:datatype xsd:string; ]
        [sh:datatype xsd:anyURI; ] );
      sh:description "This property contains the main identifier for the Catalog Record.";
      sh:maxCount 1 ;
  ] ;
  sh:property [
      sh:path dct:conformsTo ;
      sh:description "This property refers to an Application Profile that the Datasetâ€™s metadata conforms to";
      sh:datatype xsd:anyURI;
      sh:maxCount 1 ;
  ] ;
  sh:property [
      sh:path dct:conformsTo ;
      sh:datatype xsd:anyURI;
      sh:minCount 1 ;
      sh:message "ConformsTo is recommended. Please fill in a value"@en  ;
      sh:severity sh:Warning ;
  ] ;
  sh:property [
      sh:path adms:status ;
      sh:description "This property refers to the type of the latest revision of a Dataset's entry in the Catalogue. It MUST take one of the values :created, :updated or :deleted depending on whether this latest revision is a result of a creation, update or deletion.";
      sh:maxCount 1 ;
      sh:datatype xsd:anyURI;
  ] ;
  sh:property [
      sh:path adms:status ;
      sh:minCount 1 ;
      sh:datatype xsd:anyURI;
      sh:message "Status is recommended. Please fill in a value"@en  ;
      sh:severity sh:Warning ;
  ] ;
  sh:property [
      sh:path dct:issued ;
      sh:description "This property contains the date on which the description of the Dataset was included in the Catalogue.";
      sh:maxCount 1 ;
      sh:node epos:DateOrDateTimeDataType ;
  ] ;
  sh:property [
      sh:path dct:issued ;
      sh:minCount 1 ;
      sh:node epos:DateOrDateTimeDataType ;
      sh:message "Issued is recommended. Please fill in a value"@en  ;
      sh:severity sh:Warning ;
  ] ;
####
# CatalogRecord optional properties
####
  sh:property [
      sh:path dct:language ;
      sh:description "This property refers to a language used in the textual metadata describing titles, descriptions, etc. of the Dataset. This property can be repeated if the metadata is provided in multiple languages.";
      sh:class dct:LinguisticSystem ;
  ] ;
  sh:property [
      sh:path dct:source ;
      sh:description "This property refers to the original metadata that was used in creating metadata for the Dataset";
      sh:class dcat:CatalogRecord ;
      sh:maxCount 1 ;
  ] ;
  sh:property [
      sh:path dct:title ;
      sh:description "This property contains a name given to the Catalogue Record. This property can be repeated for parallel language versions of the name.";
      sh:datatype xsd:string;
    ] ;
  sh:property [
      sh:path dct:description ;
      sh:description "This property contains a free-text account of the record. This property can be repeated for parallel language versions of the description.";
      sh:datatype xsd:string;
    ] ;
.
epos:PersonShape
  a sh:NodeShape ;
  sh:targetClass schema:Person;
####
# Person mandatory properties
####
  sh:property [
      sh:path schema:identifier ;
      sh:description "This property contains the main identifier for the Person (e.g.,ORCID, ScopusID, etc.).";
      sh:or ([sh:datatype xsd:string;]
        [ sh:datatype xsd:anyURI; ]
        [sh:class schema:PropertyValue;]) ;
      sh:minCount 1 ;
  ] ;
####
# Person optional properties
####
  sh:property [
      sh:path schema:givenName ;
      sh:description "This property contains the first name of the Person.";
      sh:datatype xsd:string;
      sh:maxCount 1 ;
    ] ;
  sh:property [
      sh:path schema:familyName ;
      sh:description "This property contains the last name of the Person.";
      sh:datatype xsd:string;
      sh:maxCount 1 ;
  ] ;
  sh:property [
      sh:path schema:contactPoint ;
      sh:description "This property refers to the Contact Point role defined for the Person.";
      sh:node epos:ContactPointType ;
  ] ;
  sh:property [
      sh:path schema:address ;
      sh:description "This property contains the physical address of the Person.";
      sh:or ([sh:datatype xsd:string ;]
        [sh:class schema:PostalAddress; ] ) ;
      sh:maxCount 1 ;
  ] ;
  sh:property [
      sh:path schema:email ;
      sh:description "This property contains the email address of the Person.";
      sh:datatype xsd:string;
  ] ;
  sh:property [
      sh:path schema:qualifications ;
      sh:description "This property contains the specific qualifications of the Person.";
      sh:datatype xsd:string;
      sh:maxCount 1 ;
  ] ;
  sh:property [
      sh:path schema:telephone ;
      sh:description "This property contains the telephone number of the Person.";
      sh:datatype xsd:string;
  ] ;
  sh:property [
      sh:path schema:url ;
      sh:description "This property refers to the URL of the Person curriculum vitae.";
      sh:datatype xsd:anyURI;
      sh:maxCount 1 ;
  ] ;
  sh:property [
      sh:path schema:affiliation ;
      sh:description "This property refers to the organization to which the person is affiliated.";
      sh:class schema:Organization;
  ] ;
  sh:property [
      sh:path epos:annotation ;
      sh:class oa:Annotation;
  ] ;
.
epos:OrganizationShape
  a sh:NodeShape ;
  sh:targetClass schema:Organization;
####
# Organization mandatory properties
####
  sh:property [
      sh:path schema:identifier ;
      sh:description "This property contains the main identifier for the Organization (e.g.,PIC, ISNI, etc.).";
      sh:or ([sh:datatype xsd:string; ]
        [sh:datatype xsd:anyURI; ]
        [sh:class schema:PropertyValue; ] ) ;
      sh:minCount 1 ;
  ] ;
####
# Organization recommended properties
####
  sh:property [
      sh:path schema:legalName;
      sh:description "This property contains the official name of the Organization.";
      sh:datatype xsd:string;
      sh:maxCount 1 ;
  ] ;
  sh:property [
      sh:path schema:legalName;
      sh:description "This property contains the official name of the Organization.";
      sh:datatype xsd:string;
      sh:minCount 1 ;
      sh:message "LegalName is recommended. Please fill in a value"@en ;
      sh:severity sh:Warning ;
  ] ;
  sh:property [
      sh:path schema:leiCode;
      sh:description "This property contains the Organization identifier that uniquely identifies a legal entity as defined in ISO 17442.";
      sh:datatype xsd:string;
      sh:maxCount 1 ;
  ] ;
  sh:property [
      sh:path schema:leiCode;
      sh:datatype xsd:string;
      sh:minCount 1 ;
      sh:message "LeiCode is recommended. Please fill in a value"@en ;
      sh:severity sh:Warning ;
  ] ;
####
# Organization optional properties
####
  sh:property [
      sh:path epos:annotation ;
      sh:class oa:Annotation;
  ] ;
  sh:property [
      sh:path schema:address;
      sh:description "This property contains the physical address of the Organization.";
        sh:or ( [sh:datatype xsd:string ; ]
        [ sh:class schema:PostalAddress; ] ) ;
      sh:maxCount 1 ;
  ] ;
  sh:property [
      sh:path schema:logo ;
      sh:description "This property contains the URL of the Organization logo.";
      sh:datatype xsd:anyURI;
      sh:maxCount 1 ;
  ] ;
  sh:property [
      sh:path schema:url ;
      sh:description "This property contains the URL of the Organization website.";
      sh:datatype xsd:anyURI;
      sh:maxCount 1 ;
  ] ;
  sh:property [
      sh:path schema:email ;
      sh:description "This property contains the email address of the Organization.";
      sh:datatype xsd:string;
  ] ;
  sh:property [
      sh:path schema:telephone ;
      sh:description "This property contains the telephone number of the Organization.";
      sh:datatype xsd:string;
  ] ;
  sh:property [
      sh:path schema:contactPoint ;
      sh:description "This property refers to the Contact Point defined for the Organization.";
      sh:node epos:ContactPointType ;
  ] ;
  sh:property [
      sh:path schema:memberOf;
      sh:description "This property refers to an Organization to which this Organization belongs.";
      sh:class schema:Organization;
   ] ;
  sh:property [
      sh:path schema:owns;
      sh:description "This property refers to a Facility or Equipment of which it is the owner.";
      sh:or ( [sh:class epos:Facility; ]
        [sh:class epos:Equipment; ]);
  ] ;
  sh:property [
      sh:path epos:associatedProject;
      sh:description "This property refers to a Project in which the Organization is involved.";
      sh:class foaf:Project;
  ];
.
epos:ContactPointShape
  a sh:NodeShape ;
  sh:targetClass schema:ContactPoint;
####
# ContactPoint recommended properties
###
  sh:property [
      sh:path schema:contactType ;
      sh:datatype xsd:string;
      sh:minCount 1 ;
      sh:message "contactType is recommended. Please fill in a value (e.g. legalContact, financialContact, scientificContact, manager)"@en  ;
      sh:severity sh:Warning ;
      #sh:in ( "legalContact" "financialContact" "scientificContact" "manager" );
  ] ;
  sh:property [
      sh:path schema:contactType ;
      sh:description "This property contains information about the role of the Contact Point.";
      sh:datatype xsd:string;
      sh:maxCount 1 ;
  ] ;
####
# ContactPoint optional properties
####
  sh:property [
      sh:path schema:name ;
