<epos:Seismology> a skos:ConceptScheme;
	dct:title "Seismology";
	dct:description "It contains the concepts of Seismology";
.

<epos:Seismologicalproducts> a skos:Concept;
	skos:prefLabel "Seismological products";
	skos:definition "Results of extensive research and technological activies based on the data.";
	skos:inScheme <epos:Seismology>;
	skos:narrower <epos:Historicalearthquakes>, <epos:Macroseismicdata>, <epos:StudiesonEarthquakes>;
.

<epos:Historicalearthquakes> a skos:Concept;
	skos:prefLabel "Historical earthquakes";
	skos:definition "Earthquakes described by a set of numerical parameters such as origin time, epicentre and some description of its effects, most notably magnitude, number of Macroseismic Data Points (MDP) available, and the maximum observed intensity.";
	#skos:inScheme <epos:Seismologicalproducts>;
	skos:broader <epos:Seismologicalproducts>;
	skos:inScheme <epos:Seismology>;
.

<epos:Macroseismicdata> a skos:Concept;
	skos:prefLabel "Macroseismic Data Points";
	skos:definition "The study of any effects of earthquakes that are observable without instruments, such as felt by people, landslides, fissures, knocked-down chimneys, etc. Semi-quantitative expression of the earthquake effects, as derived from written accounts, questionnaires, etc, formalized in terms of macroseismic intensity at a given location. Usually abbreviated as MDP.";
	#skos:inScheme <epos:Seismologicalproducts>; 
	skos:broader <epos:Seismologicalproducts>;
	skos:inScheme <epos:Seismology>;
.

<epos:Studiesonearthquakes> a skos:Concept;
	skos:prefLabel "Studies on earthquakes";
	skos:definition "Published results of scientific investigations.";
	#skos:inScheme <epos:Seismologicalproducts>;
	skos:broader <epos:Seismologicalproducts>;
	skos:inScheme <epos:Seismology>;
.

