# OPERATIONS (#1) => FDSNWS-Event
<https://www.epos-eu.org/epos-dcat-ap/Seismology/Dataset/001/EMSC/operation> a hydra:Operation;
	hydra:method "GET"^^xsd:string;
    #hydra:returns "json, QuakeML";
	hydra:returns "application/xml";
	hydra:property[ a hydra:IriTemplate;
	hydra:template "http://www.seismicportal.eu/fdsnws/event/1/query{?starttime, endtime, minlatitude, maxlatitude, minlongitude, maxlongitude, mindepth, maxdepth, minmagnitude, maxmagnitude, includeallorigins, includearrivals, eventid, limit, offset, orderby, contributor, catalog, updatedafter, format, nodata, latitude, longitude, minradius, maxradius}"^^xsd:string;
		
		# /!\ Take into account that "includeallmagnitudes" parameters is not included in EMSC FDSNWS-Event... /!\
		
		hydra:mapping[ a hydra:IriTemplateMapping;
			hydra:variable "starttime"^^xsd:string;			
			hydra:property "schema:startDate";
			schema:valuePattern "YYYY-MM-DDTHH:MM:SSZ";	
			rdfs:range "xsd:dateTime";
			rdfs:label "Start Time";
			schema:minValue "1998-01-01T00:00:00";
			schema:maxValue "2018-12-31T00:00:00";
			schema:defaultValue "2010-01-00T00:00:00";
			hydra:required "false"^^xsd:boolean;
		];
		
		hydra:mapping[ a hydra:IriTemplateMapping;
			hydra:variable "endtime"^^xsd:string;
		    hydra:property "schema:endDate";
			schema:valuePattern "YYYY-MM-DDThh:mm:ssZ";
			rdfs:range "xsd:dateTime";
			rdfs:label "End Time";
			schema:minValue "1998-01-01T00:00:00";
			schema:maxValue "2018-12-31T00:00:00";
			schema:defaultValue "2010-01-00T00:00:00";
			hydra:required "false"^^xsd:boolean;
		];
		
		hydra:mapping[ a hydra:IriTemplateMapping;
			hydra:variable "minlatitude"^^xsd:string;
			rdfs:range "xsd:float";
			rdfs:label "Minimum Latitude";
			schema:minValue "-90";
			schema:maxValue "90";
			schema:defaultValue "48";
			hydra:required "false"^^xsd:boolean;
		];
		
		hydra:mapping[ a hydra:IriTemplateMapping;
			hydra:variable "maxlatitude"^^xsd:string;
			rdfs:range "xsd:float";
			rdfs:label "Maximum Latitude";
			schema:minValue "-90";
			schema:maxValue "90";
			schema:defaultValue "48";
			hydra:required "false"^^xsd:boolean;
		];
		
		hydra:mapping[ a hydra:IriTemplateMapping;
			hydra:variable "minlongitude"^^xsd:string;
			rdfs:range "xsd:float";
			rdfs:label "Minimum Longitude";
			schema:minValue "-180";
			schema:maxValue "180";
			schema:defaultValue "2";
			hydra:required "false"^^xsd:boolean;
		];
		
		hydra:mapping[ a hydra:IriTemplateMapping;
			hydra:variable "maxlongitude"^^xsd:string;
			rdfs:range "xsd:float";
			rdfs:label "Maximum Longitude";
			schema:minValue "-180";
			schema:maxValue "180";
			schema:defaultValue "2";
			hydra:required "false"^^xsd:boolean;
		];
		
		hydra:mapping[ a hydra:IriTemplateMapping;
			hydra:variable "mindepth"^^xsd:string;
			rdfs:range "xsd:integer";
			rdfs:label "Minimum Depth";
			schema:minValue "0";
			schema:maxValue "6371";
			schema:defaultValue "10";
			hydra:required "false"^^xsd:boolean;
		];
		
		hydra:mapping[ a hydra:IriTemplateMapping;
			hydra:variable "maxdepth"^^xsd:string;
			rdfs:range "xsd:integer";
			rdfs:label "Maximum Depth";
			schema:minValue "0";
			schema:maxValue "6371";
			schema:defaultValue "10";
			hydra:required "false"^^xsd:boolean;
		];
		
		hydra:mapping[ a hydra:IriTemplateMapping;
			hydra:variable "minmagnitude"^^xsd:string;
			rdfs:range "xsd:float";
