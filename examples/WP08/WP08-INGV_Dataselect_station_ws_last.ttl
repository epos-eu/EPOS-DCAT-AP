				http:paramValue "channel";
				http:paramValue "response";
				hydra:required "true"^^xsd:boolean;
				schema:defaultValue "station" ;
			];
			hydra:mapping[ a hydra:IriTemplateMapping;
				hydra:variable "nodata"^^xsd:string;
