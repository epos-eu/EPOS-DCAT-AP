	<anthropogenic_hazards/dataset/episode/NORTHWICH> a dcat:Dataset;
		dct:title "NORTHWICH: underground salt extraction cavities";
		dct:identifier "anthropogenic_hazards/dataset/episode/NORTHWICH";
