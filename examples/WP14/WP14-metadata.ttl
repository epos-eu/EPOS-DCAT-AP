	<anthropogenic_hazards/webservice/is-epos_platform/episodes> a hydra:Operation;
		hydra:method "GET"^^xsd:string;
		hydra:returns "application/json";
		hydra:property[ a hydra:IriTemplate;
			hydra:template "https://tcs.ah-epos.eu/api/epos/episodes{?impactingFactor}"^^xsd:string;
				hydra:mapping[ a hydra:IriTemplateMapping;
					hydra:variable "impactingFactor"^^xsd:string;
					rdfs:range "xsd:string";
					rdfs:label "Impacting Factor of the episode";
					http:paramValue "CO2 sequestration";
					http:paramValue "conventional hydrocarbon extraction";
					http:paramValue "geothermal energy production";
					http:paramValue "reservoir impoundment";
					http:paramValue "unconventional hydrocarbon extraction";
					http:paramValue "underground gas storage";
					http:paramValue "underground mining";
					http:paramValue "wastewater injection";
					hydra:required "false"^^xsd:boolean;
				];
		] ;
. 
	<anthropogenic_hazards/webservice/is-epos_platform/apps> a hydra:Operation;
		hydra:method "GET"^^xsd:string;
		hydra:returns "application/json";
		hydra:property[ a hydra:IriTemplate;
			hydra:template "https://tcs.ah-epos.eu/api/epos/apps{?category, keyword}"^^xsd:string;
				hydra:mapping[ a hydra:IriTemplateMapping;
					hydra:variable "category"^^xsd:string;
					rdfs:range "xsd:string";
					rdfs:label "Category to filter applications";
					http:paramValue "Collective Properties of Seismicity";
					http:paramValue "Converters";
					http:paramValue "Correlation Analysis";
					http:paramValue "Data Processing Applications";
					http:paramValue "Download Tools";
					http:paramValue "Earthquake Interactions";
					http:paramValue "Event Detection Algorithms";
					http:paramValue "Filtering Tools";
					http:paramValue "Probabilistic Seismic Hazard Analysis";
					http:paramValue "Seismogram Analysis Tools";
					http:paramValue "Source Parameter Estimation";
					http:paramValue "Stress Field Modeling";
					http:paramValue "Visualizations";
					hydra:required "false"^^xsd:boolean;
				];
				hydra:mapping[ a hydra:IriTemplateMapping;
					hydra:variable "keyword"^^xsd:string;
					rdfs:range "xsd:string";
