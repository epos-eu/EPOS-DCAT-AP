	<anthropogenic_hazards/dataset/episode/GISOS-CERVILLE> a dcat:Dataset;
		dct:title "GISOS-CERVILLE: underground solution mining";
		dct:identifier "anthropogenic_hazards/dataset/episode/GISOS-CERVILLE";
		adms:identifier [ a adms:Identifier;
			adms:schemaAgency "DDSS-ID";
