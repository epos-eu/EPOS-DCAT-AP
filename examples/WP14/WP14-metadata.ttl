	<anthropogenic_hazards/software/GDFTDSH> a schema:SoftwareApplication;
		schema:identifier "anthropogenic_hazards/software/GDFTDSH" ;
		schema:identifier [ a schema:PropertyValue ;
			schema:propertyID  "DDSS-ID" ;
		  	schema:value "WP14-DDSS-A19" ;
		];
		schema:name "Estimation of Source Parameters in Time-varying Production Parameters Geometry" ;
		schema:description "To estimate time-dependent values of seismic hazard parameters: the activity rate, the Gutenberg-Richter b-value, the return period and the exceedance probability for a prescribed area. The parameter values for the time moment t are estimated optionally either from events that occurred in dt time units preceding t, where dt is kept constant, or from n last events before t, where n is kept constant. Four magnitude distribution estimation methods are supported: maximum likelihood using the unbounded Gutenberg-Richter relation based model, maximum likelihood using the upper-bounded Gutenberg-Richter relation based model, unbounded non-parametric kernel estimation, upper-bounded non-parametric kernel estimation. The upper limit of magnitude distribution is evaluated using the Kijko-Sellevol generic formula." ;
  		schema:contactPoint  <https://orcid.org/0000-0002-9824-7962/contactPoint> ;
		schema:mainEntityOfPage "https://tcs.ah-epos.eu/#app:GDFTDSH"^^xsd:anyURI ;
		schema:license "https://creativecommons.org/licenses/by/4.0/"^^xsd:anyURI ;
		schema:softwareVersion "1.0";
		schema:keywords "statistical analysis", "probabilistic seismic hazard analysis", "time-dependent hazard", "production-dependent hazard", "production - seismicity interaction";
		dcat:theme <epos:ProbabilisticSeismicHazardAnalysis>;
.

	<anthropogenic_hazards/software/TimeVaryingFullTDSH> a schema:SoftwareApplication;
		schema:identifier "anthropogenic_hazards/software/TimeVaryingFullTDSH" ;
		schema:identifier [ a schema:PropertyValue ;
			schema:propertyID  "DDSS-ID" ;
		  	schema:value "WP14-DDSS-A20" ;
		];
		schema:name "Time Dependent Hazard in Mining Front Surroundings" ;
		schema:description "To estimate time-dependent values of seismic hazard parameters: the activity rate, the Gutenberg-Richter b-value, the return period and the exceedance probability for a space window surrounding an advancing mining front. The parameter values for the time moment t are estimated optionally either from events that occurred in dt time units preceding t, where dt is kept constant, or from n last events before t, where n is kept constant, or from a time period during which the front advanced by a constant length dx. Four magnitude distribution estimation methods are supported: maximum likelihood using the unbounded Gutenberg-Richter relation based model, maximum likelihood using the upper- bounded Gutenberg-Richter relation based model, unbounded non-parametric kernel estimation, upper-bounded non-parametric kernel estimation. The upper limit of magnitude distribution is evaluated using the Kijko-Sellevol generic formula." ;
  		schema:contactPoint  <https://orcid.org/0000-0002-9824-7962/contactPoint> ;
		schema:mainEntityOfPage "https://tcs.ah-epos.eu/#app:TimeVaryingFullTDSH"^^xsd:anyURI ;
		schema:license "https://creativecommons.org/licenses/by/4.0/"^^xsd:anyURI ;
		schema:softwareVersion "1.0";
		schema:keywords "statistical analysis", "probabilistic seismic hazard analysis", "time-dependent hazard", "production-dependent hazard", "production - seismicity interaction";
		dcat:theme <epos:ProbabilisticSeismicHazardAnalysis>;
.

