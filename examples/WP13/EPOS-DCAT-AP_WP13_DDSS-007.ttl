###############################################################################

<https://www.epos-eu.org/epos-dcat-ap/GeoElectroMagnetism/WP13-DDSS-007/Dataset> a dcat:Dataset;
    dct:identifier "https://www.epos-eu.org/epos-dcat-ap/GeoElectroMagnetism/WP13-DDSS-007/Dataset";
    dct:title "ISGI Geomagnetic Events Data";
    dct:description "Since 2015, International Service of Geomagnetic Indices is hosted by EOST, Strasbourg, France. ISGI is a service of the International Association of Geomagnetism and Aeronomy (IAGA). ISGI is the Reference Service for validation, dissemination and stewardship of geomagnetic indices through its official Web portal.";
    adms:identifier [ a adms:Identifier;
        adms:schemaAgency "DDSS-ID";
        skos:notation "WP13-DDSS-007";
    ];
    dct:created "2018-08-31"^^xsd:date;
    dct:modified "2018-08-31"^^xsd:date;
    dct:issued "2018-08-31"^^xsd:date;
    owl:versionInfo "1.0.0" ;

    dct:type "http://purl.org/dc/dcmitype/Collection"^^xsd:anyURI ;
    dct:accrualPeriodicity "http://purl.org/cld/freq/continuous"^^xsd:anyURI ;
    dcat:theme  <#Magneticactivity> ;
    dcat:keyword "DISTURBED DAY", "GEO-EFFECTIVITY", "GEOMAGNETIC ACTIVITY", "GEOMAGNETIC STORM", "GEOMAGNETISM", "INTERPLANETARY MAGNETIC FIELD (IMF)", "IONOSPHERE", "MAGNETIC FIELD", "MAGNETISM", "MAGNETOSPHERE", "QUIET DAY", "RAPID MAGNETIC VARIATIONS", "REMARKABLE EVENT", "SOLAR FLARE", "SOLAR FLARE EFFECT", "SOLAR QUIET", "SOLAR STORM", "SPACE-WEATHER", "SUDDEN STORM COMMENCEMENT", "SUDDEN COMMENCEMENT", "SUDDEN IMPULSE", "TIME SERIES";
    dcat:contactPoint <http://orcid.org/0000-0001-8793-1315/contactPoint>;
    dct:publisher <PIC:999997930>;
    dcat:distribution <https://www.epos-eu.org/epos-dcat-ap/GeoElectroMagnetism/WP13-DDSS-007/Distribution>;

    dct:temporal [ a dct:PeriodOfTime;
        schema:startDate "1868-01-01T00:00:00Z"^^xsd:dateTime;
        #schema:endDate "YYYY-MM-DDThh:mm:ssZ"^^xsd:dateTime;
    ];
    dct:spatial [ a dct:Location;
        locn:geometry "POLYGON(180.0 -90.0, -180.0 -90.0, -180.0 90.0, 180.0 90.0, 180.0 -90.0)"^^gsp:wktLiteral;
    ];
.

