    schema:serialNumber "NL.HGN..BHE" ;

    # This property refers to an Equipment or a Facility in which the described Equipment is included.
    dct:isPartOf <https://www.epos-eu.org/epos-dcat-ap/Seismology/Facility/001> ;

    dcat:contactPoint <http://orcid.org/0000-0002-6250-2474/contactPoint> ;

