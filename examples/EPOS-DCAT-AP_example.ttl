    dct:spatial [ a dct:Location ;
            locn:geometry "POINT(50.764 5.9317 135.0)"^^gsp:wktLiteral ;
            ] ;

    # This property describes the filter that the instrument uses to produce data.
    epos:filter "filter";

    # This property contains the full-scale measuring ability (unit and value).
    epos:dynamicRange [ a schema:QuantitativeValue ;
        	schema:unitCode "unit code"; # http://wiki.goodrelations-vocabulary.org/Documentation/UN/CEFACT_Common_Codes
