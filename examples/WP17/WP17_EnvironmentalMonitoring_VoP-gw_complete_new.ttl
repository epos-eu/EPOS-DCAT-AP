#########
# Dataset
######### 

<BGS_VoP_GW> a dcat:Dataset;
	dct:title "Environmental baseline monitoring in the Vale of Pickering (UK) - groundwater quality data (in real time)";
	dct:identifier "BGS_VoP_GW";
	adms:identifier [ a adms:Identifier;
		adms:schemaAgency "DDSS-ID";
		skos:notation "WP17-DDSS-004";
	];
	dct:description "Real time groundwater quality data forming part of environmental monitoring undertaken in the Vale of Pickering (UK), from November 2016 to September 2018, and on-going";
	dct:created "2016-11-01"^^xsd:date;
	dct:modified "2016-11-01"^^xsd:date;
	dct:issued "2016-11-01"^^xsd:date;
dct:temporal [ a dct:PeriodOfTime;
	schema:startDate "2016-11-01T00:00:00Z"^^xsd:dateTime;
];
dct:spatial [ a dct:Location;
	locn:geometry "POLYGON((-1.233142 54.303168,-0.311432 54.303168,-0.311432 54.123437,-1.233142 54.123437,-1.233142 54.303168))"^^gsp:wktLiteral;
];
	dcat:theme <epos:shale>;
	dcat:keyword "environmental monitoring", "baseline", "real time", "ground water", "Vale of Pickering";
	dcat:contactPoint <rswa@bgs.ac.uk/contactPoint>;
	dct:publisher <http://viaf.org/viaf/136554353/>;
	dcat:distribution <WP17-004-Download>;
.

