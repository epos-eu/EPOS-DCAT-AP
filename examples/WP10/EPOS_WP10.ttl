#WS Operation products/timeseries
<GlassFramework/webresources/products/timeseries/operation> a hydra:Operation;
 	hydra:method "GET"^^xsd:string;
	hydra:returns "json";
 	hydra:IriTemplate[ a hydra:IriTemplate;
	rdf:label "The optional parameters should be separated by & (AMPERSAND)";
	rdf:label "https://glass.epos.ubi.pt:8080/GlassFramework/webresources/products/timeseries/BLIX/UGA/daily/json?epoch_start=2015-02-23&remove_outliers=0";
	hydra:template "https://glass.epos.ubi.pt:8080/GlassFramework/webresources/products/timeseries/{station}/{analysis_centre}/{sampling_period}/{coordinates_system}/{format}/{opcional_parameters}"^^xsd:string;
      hydra:mapping[ a hydra:IriTemplateMapping;
      		hydra:variable "station"^^xsd:string;
					rdf:label "Four letter station identification";
          hydra:required "true"^^xsd:boolean;
					
      ];
      hydra:mapping[ a hydra:IriTemplateMapping;
          hydra:variable "analysis_centre"^^xsd:string;
					rdf:label "analysis_centre";
          hydra:required "true"^^xsd:boolean;
					
      ];
			hydra:mapping[ a hydra:IriTemplateMapping;
          hydra:variable "sampling_period"^^xsd:string;
					rdf:label "sampling_period";
          hydra:required "true"^^xsd:boolean;
					http:paramValue "daily";
					http:paramValue "weekly";
      ];
			hydra:mapping[ a hydra:IriTemplateMapping;
          hydra:variable "coordinates_system"^^xsd:string;
					rdf:label "coordinates_system";
          hydra:required "true"^^xsd:boolean;
					http:paramValue "xyz";
					http:paramValue "enu";
      ];
			hydra:mapping[ a hydra:IriTemplateMapping;
          hydra:variable "format"^^xsd:string;
					rdf:label "format";
          hydra:required "true"^^xsd:boolean;
					http:paramValue "json";
					http:paramValue "xml";
      ];
			hydra:mapping[ a hydra:IriTemplateMapping;
        	hydra:variable "reference_frame"^^xsd:string;
					rdf:label "reference_frame";
          hydra:required "false"^^xsd:boolean;
					http:paramValue "IGS08";
					http:paramValue "IGS14";
					http:paramValue "free-network";
					http:paramValue "IGb08";
					http:paramValue "INGV_EU";
      ];
			hydra:mapping[ a hydra:IriTemplateMapping;
          hydra:variable "otl_model"^^xsd:string;
					rdf:label "otl_model";
          hydra:required "false"^^xsd:boolean;
      ];
			hydra:mapping[ a hydra:IriTemplateMapping;
          hydra:variable "calibration_model"^^xsd:string;
					rdf:label "calibration_model";
          hydra:required "false"^^xsd:boolean;
      ];
			hydra:mapping[ a hydra:IriTemplateMapping;
          hydra:variable "cut_of_angle"^^xsd:string;
					rdf:label "cut_of_angle";
          hydra:required "false"^^xsd:boolean;
      ];
			hydra:mapping[ a hydra:IriTemplateMapping;
					hydra:variable "remove_outliers"^^xsd:string;
					rdf:label "remove_outliers - ( If nothing is given, outliers are removed by default) ";
					hydra:required "false"^^xsd:boolean;
					http:paramValue "1";
					http:paramValue "0";				
      ];
			hydra:mapping[ a hydra:IriTemplateMapping;
					hydra:variable "apply_offsets"^^xsd:string;
					rdf:label "apply_offsets - (1 to apply)";
					hydra:required "false"^^xsd:boolean;
					http:paramValue "1";				
      ];
			hydra:mapping[ a hydra:IriTemplateMapping;
					hydra:variable "epoch_start"^^xsd:string;
					rdf:label "epoch_start (date format YYYY-MM-DD)";
					hydra:required "false"^^xsd:boolean;				
      ];
			hydra:mapping[ a hydra:IriTemplateMapping;
					hydra:variable "epoch_end"^^xsd:string;
					rdf:label "epoch_end (date format YYYY-MM-DD)";
					hydra:required "false"^^xsd:boolean;				
      ];
    ];
.
