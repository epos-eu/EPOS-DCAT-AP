	schema:description "get coordinates object for a given parameters.";
 schema:name "GET Coordinates";
 dcat:contactPoint <http://orcid.org/0000-0002-9733-284X/contactPoint> ;
 schema:datePublished "2016-06-30T12:00:00Z"^^xsd:dateTime;
 schema:dateModified "2016-12-31T12:00:00Z"^^xsd:dateTime;
 schema:provider <PIC:996437254>;
 dcat:theme  <epos:GNSSProductsPortal> ;
 hydra:entrypoint "https://glass.epos.ubi.pt:8080/GlassFramework/swagger.json"^^xsd:anyURI;
	schema:keywords "Geodesy", "GNSS" ;
 dct:conformsTo <GlassFramework/APIDocumentation> ;
 hydra:supportedOperation <GlassFramework/webresources/products/coordinates/operation>;
.

#WS Operation products/coordinates
<GlassFramework/webresources/products/coordinates/operation> a hydra:Operation;
	hydra:method "GET"^^xsd:string;
 	hydra:returns "application/json";
	hydra:property[ a hydra:IriTemplate;
		rdfs:label "https://glass.epos.ubi.pt:8080/GlassFramework/webresources/products/coordinates/BLIX/UGA/daily/xyz/json?epoch_start=2015-02-23&remove_outliers=1";
    hydra:template "https://glass.epos.ubi.pt:8080/GlassFramework/webresources/products/timeseries/{station}/{analysis_centre}/{sampling_period}/{coordinates_system}/{format/{?reference_frame,otl_model,calibration_model,cut_of_angle,remove_outliers,apply_offsets,epoch_start,epoch_end}"^^xsd:string;
    hydra:mapping[ a hydra:IriTemplateMapping;
      hydra:variable "station"^^xsd:string;
      rdfs:label "Four letter station identification";
      rdfs:range "xsd:string";
      hydra:required "true"^^xsd:boolean;
      schema:defaultValue "BLIX";
    ];
    hydra:mapping[ a hydra:IriTemplateMapping;
      hydra:variable "analysis_centre"^^xsd:string;
      rdfs:label "analysis_centre; use the following operation to get the all the formats available";
      rdfs:range "xsd:string";
      hydra:required "true"^^xsd:boolean;
      schema:defaultValue "UGA";
    ];
    hydra:mapping[ a hydra:IriTemplateMapping;
      hydra:variable "sampling_period"^^xsd:string;
      rdfs:label "sampling_period";
      rdfs:range "xsd:string";
      hydra:required "true"^^xsd:boolean;
      http:paramValue "daily";
      http:paramValue "weekly";
      schema:defaultValue "daily";
    ];
    hydra:mapping[ a hydra:IriTemplateMapping;
      hydra:variable "coordinates_system"^^xsd:string;
      rdfs:label "coordinates_system";
      rdfs:range "xsd:string";
      hydra:required "true"^^xsd:boolean;
      http:paramValue "xyz";
      http:paramValue "enu";
      schema:defaultValue "xyz";
    ];
    hydra:mapping[ a hydra:IriTemplateMapping;
      hydra:variable "format"^^xsd:string;
      rdfs:label "format; use the following operation to get the all the formats available";
      rdfs:range "xsd:string";
      hydra:required "true"^^xsd:boolean;
      schema:defaultValue "json";
    ];
    hydra:mapping[ a hydra:IriTemplateMapping;
      hydra:variable "reference_frame"^^xsd:string;
      rdfs:label "reference_frame; use the following operation to get the all the reference frames available";
      rdfs:range "xsd:string";
      hydra:required "false"^^xsd:boolean;
    ];
    hydra:mapping[ a hydra:IriTemplateMapping;
      hydra:variable "otl_model"^^xsd:string;
      rdfs:label "otl_model; use the following operation to get the all the otl models available";
      rdfs:range "xsd:string";
      hydra:required "false"^^xsd:boolean;
    ];
    hydra:mapping[ a hydra:IriTemplateMapping;
      hydra:variable "calibration_model"^^xsd:string;
      rdfs:label "calibration_model; use the following operation to get the all the calibration models available ";
      rdfs:range "xsd:string";
      hydra:required "false"^^xsd:boolean;
    ];
    hydra:mapping[ a hydra:IriTemplateMapping;
      hydra:variable "cut_of_angle"^^xsd:string;
      rdfs:label "cut_of_angle";
      rdfs:range "xsd:double";
      hydra:required "false"^^xsd:boolean;
    ];
    hydra:mapping[ a hydra:IriTemplateMapping;
      hydra:variable "remove_outliers"^^xsd:string;
      rdfs:label "remove_outliers";
      schema:defaultValue "1";
      rdfs:range "xsd:boolean";
      hydra:required "false"^^xsd:boolean;
      http:paramValue "1";
      http:paramValue "0";
    ];
    hydra:mapping[ a hydra:IriTemplateMapping;
      hydra:variable "apply_offsets"^^xsd:string;
      rdfs:label "apply_offsets";
      schema:defaultValue "0";
      rdfs:range "xsd:boolean";
      hydra:required "false"^^xsd:boolean;
      http:paramValue "1";
      http:paramValue "0";
    ];
    hydra:mapping[ a hydra:IriTemplateMapping;
      hydra:variable "epoch_start"^^xsd:string;
      rdfs:label "epoch_start (date format YYYY-MM-DD)";
      rdfs:range "xsd:dateTime";
      hydra:required "false"^^xsd:boolean;
    ];
    hydra:mapping[ a hydra:IriTemplateMapping;
      hydra:variable "epoch_end"^^xsd:string;
      rdfs:label "epoch_end (date format YYYY-MM-DD)";
      rdfs:range "xsd:dateTime";
      hydra:required "false"^^xsd:boolean;
    ];
];
.

