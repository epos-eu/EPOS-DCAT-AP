	dcat:contactPoint <http://orcid.org/0000-0002-9733-284X/contactPoint> ;
	schema:datePublished "2016-06-30T12:00:00Z"^^xsd:dateTime;
	schema:dateModified "2016-12-31T12:00:00Z"^^xsd:dateTime;
	schema:provider <PIC:996437254>;
	dcat:theme  <epos:GNSSProductsPortal> ;
	hydra:entrypoint "https://glass.epos.ubi.pt:8080/GlassFramework/swagger.json"^^xsd:anyURI;
	schema:keywords "Geodesy", "GNSS" ;
	dct:conformsTo <GlassFramework/APIDocumentation> ;
	hydra:supportedOperation <GlassFramework/webresources/products/stations/operation>;
.

#WS Operation products/stations
<GlassFramework/webresources/products/stations/operation> a hydra:Operation;
	hydra:method "GET"^^xsd:string;
	hydra:returns "application/json";
	hydra:property[ a hydra:IriTemplate;
		#hydra:template "https://glass.epos.ubi.pt:8080/GlassFramework/webresources/products/stations{?analysis_center,reference_frame,otl_model,calibration_model,cut_of_angle,epoch_start,epoch_end,coordinates}"^^xsd:string;
		hydra:template "https://glass.epos.ubi.pt:8080/GlassFramework/webresources/products/stations{?analysis_center,reference_frame,otl_model,calibration_model,cut_of_angle,epoch_start,epoch_end}&coordinates_rectangle={min_lat,max_lat,min_lon,max_lon}"^^xsd:string;

		hydra:mapping[ a hydra:IriTemplateMapping;
			hydra:variable "analysis_center"^^xsd:string;
			rdfs:label "analysis_center; use the following operation to get the all the formats available";
			rdfs:range "xsd:string";
			hydra:required "false"^^xsd:boolean;
      		schema:defaultValue "WUT-EUREF";
		];
		hydra:mapping[ a hydra:IriTemplateMapping;
			hydra:variable "reference_frame"^^xsd:string;
			rdfs:label "reference_frame; use the following operation to get the all the reference frames available";
			rdfs:range "xsd:string";
			hydra:required "false"^^xsd:boolean;
		];
		hydra:mapping[ a hydra:IriTemplateMapping;
			hydra:variable "otl_model"^^xsd:string;
			rdfs:label "otl_model; use the following operation to get the all the otl models available";
			rdfs:range "xsd:string";
			hydra:required "false"^^xsd:boolean;
		];
		hydra:mapping[ a hydra:IriTemplateMapping;
			hydra:variable "calibration_model"^^xsd:string;
			rdfs:label "calibration_model; use the following operation to get the all the calibration models available ";
			rdfs:range "xsd:string";
			hydra:required "false"^^xsd:boolean;
		];
		hydra:mapping[ a hydra:IriTemplateMapping;
			hydra:variable "cut_of_angle"^^xsd:string;
			rdfs:label "cut_of_angle";
			rdfs:range "xsd:double";
			hydra:required "false"^^xsd:boolean;
		];
		hydra:mapping[ a hydra:IriTemplateMapping;
			hydra:variable "epoch_start"^^xsd:string;
			rdfs:label "epoch_start (date format YYYY-MM-DD)";
			rdfs:range "xsd:dateTime";
			hydra:property "schema:startDate";
			schema:valuePattern "YYYY-MM-DD";
			hydra:required "false"^^xsd:boolean;
      		schema:defaultValue "2015-02-23";
		];
		hydra:mapping[ a hydra:IriTemplateMapping;
			hydra:variable "epoch_end"^^xsd:string;
			rdfs:label "epoch_end (date format YYYY-MM-DD)";
			rdfs:range "xsd:dateTime";
			hydra:property "schema:endDate";
			schema:valuePattern "YYYY-MM-DD";
			hydra:required "false"^^xsd:boolean;
			schema:defaultValue "2015-03-31";
		];
		#hydra:mapping[ a hydra:IriTemplateMapping;
			#hydra:variable "coordinates"^^xsd:string;
      			#rdfs:label	"coordinates";
      			#rdfs:range "xsd:string";
      			#http:paramValue	"rectangle";
			#http:paramValue	"circle";
			#http:paramValue	"polygon";
      			#hydra:required "false"^^xsd:boolean;
		#];
		#At this stage we consider only web services that support the spatial query by using rectangle
		hydra:mapping[ a hydra:IriTemplateMapping;
			hydra:variable "min_lon"^^xsd:string;
			hydra:property "epos:westernmostLongitude" ;
      			rdfs:label "minLon";
			schema:minValue "-180" ;
      			schema:maxValue "180" ;
		 	hydra:required "false"^^xsd:boolean;
		];
		hydra:mapping[ a hydra:IriTemplateMapping;
			hydra:variable "min_lat"^^xsd:string;
			hydra:property "epos:southernmostLatitude" ;
      			rdfs:label "minLat";
			schema:minValue "-90" ;
      			schema:maxValue "90" ;
		 	hydra:required "false"^^xsd:boolean;
		];
		hydra:mapping[ a hydra:IriTemplateMapping;
			hydra:variable "max_lon"^^xsd:string;
			hydra:property "epos:easternmostLongitude" ;
      			rdfs:label "maxLon";
			schema:minValue "-180" ;
      			schema:maxValue "180" ;
		 	hydra:required "false"^^xsd:boolean;
		];
		hydra:mapping[ a hydra:IriTemplateMapping;
			hydra:variable "max_lat"^^xsd:string;
			hydra:property "epos:northernmostLatitude" ;
      			rdfs:label "maxLat";
			schema:minValue "0" ;
      			schema:maxValue "90" ;
		 	hydra:required "false"^^xsd:boolean;
		];

	
	];
.

