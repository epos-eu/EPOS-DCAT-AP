#WS Operation products/timeseries
<GlassFramework/webresources/products/timeseries/operation> a hydra:Operation;
	hydra:method "GET"^^xsd:string;
	hydra:returns "json/xml";
	hydra:IriTemplate[ a hydra:IriTemplate;
			rdf:label "https://glass.epos.ubi.pt:8080/GlassFramework/webresources/products/timeseries/BLIX/UGA/daily/xyz/json?epoch_start=2015-02-23&remove_outliers=1";
			hydra:template "https://glass.epos.ubi.pt:8080/GlassFramework/webresources/products/timeseries/{station}/{analysis_centre}/{sampling_period}/{coordinates_system}/{format}?{optional_parameters}"^^xsd:string;
		 	hydra:mapping[ a hydra:IriTemplateMapping;
				hydra:variable "station"^^xsd:string;
				rdf:label "Four letter station identification";
				rdfs:range "xds:string";
				hydra:required "true"^^xsd:boolean;
			 
		 	];
		 	hydra:mapping[ a hydra:IriTemplateMapping;
				hydra:variable "analysis_centre"^^xsd:string;
				rdf:label "analysis_centre; use the following operation to get the all the formats available";
				hydra:supportedOperation <GlassFramework/webresources/products/attributes/operation>;
				rdfs:range "xds:string";
				hydra:required "true"^^xsd:boolean;
			 
		 	];
		 	hydra:mapping[ a hydra:IriTemplateMapping;
				hydra:variable "sampling_period"^^xsd:string;
				rdf:label "sampling_period";
				rdfs:range "xds:string";
				hydra:required "true"^^xsd:boolean;
				http:paramValue "daily";
				http:paramValue "weekly";
		 	];
		 	hydra:mapping[ a hydra:IriTemplateMapping;
				hydra:variable "coordinates_system"^^xsd:string;
				rdf:label "coordinates_system";
				rdfs:range "xds:string";
				hydra:required "true"^^xsd:boolean;
				http:paramValue "xyz";
				http:paramValue "enu";
		 	];
		 	hydra:mapping[ a hydra:IriTemplateMapping;
				hydra:variable "format"^^xsd:string;
				rdf:label "format; use the following operation to get the all the formats available";
				hydra:supportedOperation <GlassFramework/webresources/products/attributes/operation>;
				rdfs:range "xds:string";
				hydra:required "true"^^xsd:boolean;	
		 	];
			hydra:mapping[ a hydra:IriTemplateMapping;
				hydra:variable "optional_parameters"^^xsd:string;
				rdf:label "list of all available parameters";
				rdf:label "The optional parameters should be separated by & (AMPERSAND)";
				hydra:required "false"^^xsd:boolean;
				http:paramValue[ a hydra:IriTemplateMapping; 
					hydra:mapping[ a hydra:IriTemplateMapping;
						hydra:variable "reference_frame"^^xsd:string;
						rdf:label "reference_frame; use the following operation to get the all the reference frames available";
						hydra:supportedOperation <GlassFramework/webresources/products/attributes/operation>;
						rdfs:range "xds:string";
						hydra:required "false"^^xsd:boolean;
					];
					hydra:mapping[ a hydra:IriTemplateMapping;
						hydra:variable "otl_model"^^xsd:string;
						rdf:label "otl_model; use the following operation to get the all the otl models available";
						hydra:supportedOperation <GlassFramework/webresources/products/attributes/operation>;
						rdfs:range "xds:string";
						hydra:required "false"^^xsd:boolean;
					];
					hydra:mapping[ a hydra:IriTemplateMapping;
						hydra:variable "calibration_model"^^xsd:string;
						rdf:label "calibration_model; use the following operation to get the all the calibration models available ";
						hydra:supportedOperation <GlassFramework/webresources/products/attributes/operation>;
						rdfs:range "xds:string";
						hydra:required "false"^^xsd:boolean;
					];
					hydra:mapping[ a hydra:IriTemplateMapping;
						hydra:variable "cut_of_angle"^^xsd:string;
						rdf:label "cut_of_angle";
						rdfs:range "xsd:double";
						hydra:required "false"^^xsd:boolean;
					];
					hydra:mapping[ a hydra:IriTemplateMapping;
						hydra:variable "remove_outliers"^^xsd:string;
						rdf:label "remove_outliers";
						schema:defaultValue "1";
						rdfs:range "xds:boolean";
						hydra:required "false"^^xsd:boolean;
						http:paramValue "1";
						http:paramValue "0";				
					];
					hydra:mapping[ a hydra:IriTemplateMapping;
						hydra:variable "apply_offsets"^^xsd:string;
						rdf:label "apply_offsets";
						schema:defaultValue "0";
						rdfs:range "xds:boolean";
						hydra:required "false"^^xsd:boolean;
						http:paramValue "1";
						http:paramValue "0";				
					];
					hydra:mapping[ a hydra:IriTemplateMapping;
						hydra:variable "epoch_start"^^xsd:string;
						rdf:label "epoch_start (date format YYYY-MM-DD)";
						rdfs:range "xsd:dateTime";
						hydra:required "false"^^xsd:boolean;
					];
					hydra:mapping[ a hydra:IriTemplateMapping;
						hydra:variable "epoch_end"^^xsd:string;
						rdf:label "epoch_end (date format YYYY-MM-DD)";
						rdfs:range "xsd:dateTime";
						hydra:required "false"^^xsd:boolean;				
					];
				];
			];
	];
.

