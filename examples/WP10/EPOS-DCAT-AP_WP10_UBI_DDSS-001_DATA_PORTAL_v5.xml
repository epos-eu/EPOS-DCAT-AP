  <!-- EPOS Data Portal WebService -->
  <eposap:WebService>
    <dct:title>EPOS WP10 GNSS Data Portal</dct:title>
    <dct:description>EPOS WP10 GNSS Data Portal</dct:description>
    <dct:issued>2016-06-30T12:00:00</dct:issued>
    <dct:modified>2016-12-31T12:00:00</dct:modified>
    <dct:license>GPL, Open Data</dct:license>
    <!-- Access and Use Restrictions -->
    <foaf:page>
      <!-- URI whats the difference to distribuition below This property defines the link(s) to the resource and/or the link to additional information about the resource
-->
      <foaf:primaryTopic>http://glass.unice.fr:8080/GlassFramework</foaf:primaryTopic>
    </foaf:page>
    <dct:format>
      <!-- distribution format, in case the service provides (also) access to binary data -->
      <dct:MediaTypeOrExtent>text</dct:MediaTypeOrExtent>
      <!-- gmd is used as an xml implementation also text and binary ??-->
    </dct:format>
    <!--Spatial Reference System the CRS should be specified with the relevant URI.The base URI is: http://www.opengis.net/def/crs/ -->
    <dct:conformsTo>EPSG:3763</dct:conformsTo>
    <!-- Spatial Reference System [UBI?][http://spatialreference.org/ref/epsg/3763/] -->
    <dct:identifier>EPOS.WP10.DGW</dct:identifier>
    <!-- Unique Resource Identifier: this is the entry point of the service [UBI?]-->
    <dct:created>2017-06-30T12:00:00</dct:created>
    <!-- date of creation -->
    <eposap:domain>
		Satellite Data, Geodesy
	</eposap:domain>
    <eposap:subDomain>
      <!-- This property refers to the subdomain of resource (e.g. 
		Earthquake parameters, Velocity models, GNSS station velocities, InSAR, Meteorology, 
		Variation of geomagnetic field, 3D/4D structural models, Analog models, Numerical models, 
		Geochemical data, Borehole data, Rock sample properties) -->
			GNSS Satellite Data, Station and Equipment MetaData
		</eposap:subDomain>
    <!-- Keywords, annotation. Simple and free (web annotation ontology in the future) -->
    <dcat:keyword>GNSS</dcat:keyword>
    <!-- Keywords -->
    <eposap:operation>data, portal, products, repository, visualization, download</eposap:operation>
    <dct:hasVersion>1.0</dct:hasVersion>
    <!--<dcat:Distribuition>http://glass.unice.fr/GlassFramework</dcat:Distribuition> -->
    <dcat:contactPoint>orcid0000-0002-9733-284X</dcat:contactPoint>
    <!-- internal link to contact point (vcard:Individual) described in the same file in <eposap:Person> or maybe organization -->
    <eposap:publisher>EPOS.UBI</eposap:publisher>
    <dct:spatial>
      <!-- Geographic location/spatial extent /bounding box -->
      <dct:Location>
        <!-- POLYGON(lon1 lat1, lon2) [europe UBI?]-->
        <locn:geometry>POLYGON(-12.39 30.3, -12.39 49.0, 45.28 49.0, 45.28 30.3, -12.39 30.3)</locn:geometry>
      </dct:Location>
    </dct:spatial>
    <!-- <adms:representationTechnique>vector</adms:representationTechnique>-->
    <!-- representationTechnique not applicable for metadata -->
    <dct:temporal>
      <!-- temporal extent -->
      <dct:PeriodOfTime>
        <schema:startDate>2013-03-01T12:00:00</schema:startDate>
        <!-- 	<schema:endDate>2016-12-31T12:00:00</schema:endDate> endDate not applicable to EDSF -->
      </dct:PeriodOfTime>
    </dct:temporal>
  </eposap:WebService>
  
  
