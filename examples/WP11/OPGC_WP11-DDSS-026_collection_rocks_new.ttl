#########
# Operation
######### 

<opgc.fr/vobs/dynvolc/WP11/DDSS-026/rocks-collection> a hydra:Operation;
        hydra:method "GET"^^xsd:string;
        hydra:returns "application/json";
        hydra:property [ a hydra:IriTemplate;
            hydra:template "http://opgc.fr/vobs/rest2/req.php/dynvolc/rocks/{startDate}/{endDate}{?lat-min, lat-max, lng-min, lng-max}"^^xsd:string;
                hydra:mapping [ a hydra:IriTemplateMapping;
                    hydra:variable "startDate"^^xsd:dateTime;
                    hydra:property "schema:startDate";
                    schema:valuePattern "YYYY-MM-DDThh:mm:ssZ";
                    rdfs:label "Starting datetime of the time interval query (format YYYY-MM-DDThh:mm:ssZ)";
                    rdfs:range "xsd:dateTime";
                    schema:minValue "0001-01-01T00:00:00Z"^^xsd:dateTime;
                    schema:maxValue "9999-12-31T23:59:59Z"^^xsd:dateTime;
                    hydra:required "true"^^xsd:boolean;
                    schema:defaultValue "2015-07-30T00:00:00Z"^^xsd:dateTime;
                ];
                hydra:mapping [ a hydra:IriTemplateMapping;
                    hydra:variable "endDate"^^xsd:dateTime;
                    hydra:property "schema:endDate";
                    schema:valuePattern "YYYY-MM-DDThh:mm:ssZ";
                    rdfs:label "Ending datetime of the time interval query (format YYYY-MM-DDThh:mm:ssZ)";
                    rdfs:range "xsd:dateTime";
                    schema:minValue "0001-01-01T00:00:00Z"^^xsd:dateTime;
                    schema:maxValue "9999-12-31T23:59:59Z"^^xsd:dateTime;
                    hydra:required "true"^^xsd:boolean;
                    schema:defaultValue "2015-08-03T23:45:45Z"^^xsd:dateTime;
                ];
                hydra:mapping [ a hydra:IriTemplateMapping;
                    hydra:variable "lat-min"^^xsd:float;
                    hydra:property "epos:southernmostLatitude";
                    rdfs:label "Limit the query to data above this minimum latitude (in decimal degrees)";
                    rdfs:range "xsd:float";
                    schema:minValue "-90.0"^^xsd:float;
                    schema:maxValue "90.0"^^xsd:float;
                    hydra:required "false"^^xsd:boolean
                ];
                hydra:mapping [ a hydra:IriTemplateMapping;
                    hydra:variable "lat-max"^^xsd:float;
                    hydra:property "epos:northernmostLatitude";
                    rdfs:label "Limit the query to data under this maximum latitude (in decimal degrees)";
                    rdfs:range "xsd:float";
                    schema:minValue "-90.0"^^xsd:float;
                    schema:maxValue "90.0"^^xsd:float;
                    hydra:required "false"^^xsd:boolean
                ];
                hydra:mapping [ a hydra:IriTemplateMapping;
                    hydra:variable "lng-min"^^xsd:float;
                    hydra:property "epos:westernmostLongitude";
                    rdfs:label "Limit the query to data above this minimum longitude (in decimal degrees)";
                    rdfs:range "xsd:float";
                    schema:minValue "-180.0"^^xsd:float;
                    schema:maxValue "180.0"^^xsd:float;
                    hydra:required "false"^^xsd:boolean
                ];
                hydra:mapping [ a hydra:IriTemplateMapping;
                    hydra:variable "lng-max"^^xsd:float;
                    hydra:property "epos:easternmostLongitude";
                    rdfs:label "Limit the query to data under this maximum longitude (in decimal degrees)";
                    rdfs:range "xsd:float";
                    schema:minValue "-180.0"^^xsd:float;
                    schema:maxValue "180.0"^^xsd:float;
                    hydra:required "false"^^xsd:boolean
                ];
        ];
.

