	];
	dcat:theme <epos:volvanic-activity>;
	dcat:keyword "volcanology", "geohazards","periodic reports", "exceptional reports", "volcanic eruptions", "VONA", "civil aviation", "VAAC - Volcanic Ash Advisories Centers", "WMO - World Meteorolgical Organization";
	dct:publisher <PIC:999597417>;
	dcat:distribution <WP11/volcanic-activity/DDSS031/distribution/report>;
	#dcat:distribution <http://ws.ipgp.fr/volcano-reports/>;
	owl:versionInfo "0";
	dcat:contactPoint <https://orcid.org/0000-0002-4234-3787/contactPoint>;
.

