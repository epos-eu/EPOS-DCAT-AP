		adms:schemaAgency "DDSS-ID";
		skos:notation "WP11-DDSS-031";
	];
	dct:description "Access to the periodic and exceptional reports produced by the French volcano Observatories and the Volcano Observatory Notice dor Aviation as well.";
	dct:created "1999-01-01"^^xsd:date;
	dct:temporal [ a dct:PeriodOfTime;
		schema:startDate "1999-01-01T00:00:00Z"^^xsd:dateTime;
		#schema:endDate "YYYY-MM-DDThh:mm:ssZ"^^xsd:dateTime;
	];
