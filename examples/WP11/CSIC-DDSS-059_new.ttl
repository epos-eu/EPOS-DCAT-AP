#########
# Dataset
######### 

<WP11/HazardMap/PDCs> a dcat:Dataset;
        dct:title "PDCs Hazard maps";
        dct:identifier "WP11/HazardMap/PDCs";
        dct:description "PDCs Hazard maps";
        ## example of frequency using a controlled vocabulary
	dct:type "http://purl.org/dc/dcmitype/Collection"^^xsd:anyURI;
        dct:accrualPeriodicity "http://purl.org/cld/freq/irregular"^^xsd:anyURI;
        dct:created "2018-09-01"^^xsd:date;
        dcat:theme  <epos:Hazardmap> ;
        dcat:keyword 	"Volcanoes", "pyroclastic density currents", "PDCs", "hazard map", "volcanic hazard", "volcanology", "hazard assessment";
        dcat:contactPoint <http://orcid.org/0000-0002-8803-6504/contactPoint>;
	dct:publisher <http://isni.org/isni/0000000122036192>;
        dcat:distribution <WP11/HazardMap/PDCs/WSDistribution>;
        
.

#########
# Distribution
######### 

<WP11/HazardMap/PDCs/WSDistribution> a dcat:Distribution;
        dct:identifier "WP11/HazardMaps/PDCs/WSDistribution";
	dct:description "Hazard Maps WSDistribution";
        dct:title "PDCs Hazard maps"; 
	dct:created "2018-09-01"^^xsd:date;
	dct:type "http://publications.europa.eu/resource/authority/distribution-type/WEB_SERVICE"^^xsd:anyURI;
	dcat:accessURL <HazardMap/operation>;
	dct:conformsTo <HazardMap>;
	dct:format "http://publications.europa.eu/resource/authority/file-type/TXT"^^xsd:anyURI;
.

#########
# WebService
######### 

<HazardMap> a epos:WebService;
	schema:identifier "HazardMap";
	schema:identifier [ a schema:PropertyValue;
	schema:propertyID  "DDSS-ID";
	schema:value  "WP11-DDSS-059";
	]; 
	schema:description "Pyroclastic Density Currents hazard maps";
	schema:name "PDCs hazard maps";
	schema:datePublished "2018-09-01T00:00:00Z"^^xsd:dateTime;
	schema:dateModified "2018-09-01T12:00:00Z"^^xsd:dateTime;
	schema:provider <http://isni.org/isni/0000000122036192>;
	schema:keywords "Volcanoes", "pyroclastic density currents", "PDCs", "hazard map", "volcanic hazard", "volcanology", "hazard assessment";
	hydra:entrypoint "https://volcanboxws.obsea.es/"^^xsd:anyURI; 
	hydra:supportedOperation <HazardMap/operation>;
	dcat:theme <epos:Hazardmap>;
	dcat:contactPoint <http://orcid.org/0000-0002-8803-6504/contactPoint>;
	
.

#########
# Operation
######### 

<HazardMap/operation> a hydra:Operation;
	hydra:method "GET"^^xsd:string;
	hydra:returns "application/json";
	hydra:property[ a hydra:IriTemplate;
		hydra:template "https://volcanboxws.obsea.es:5000/maptype?type=PDC_hazard_map(&volcan=deceptionisland)"^^xsd:string;

			hydra:mapping[ a hydra:IriTemplateMapping;
				hydra:variable "volcan"^^xsd:string;
				rdfs:label "Volcano name that exposes hazard map";
				schema:defaultValue "deceptionisland";
				hydra:required "false"^^xsd:boolean;
			];
	];
. 

#########
# Concept
######### 

<epos:VolcanoObservations> a skos:ConceptScheme;
	dct:title "VolcanoObservations";
	dct:description "It contains the concepts of Volcano Observations";
. 
<epos:Hazardmap> a skos:Concept;
	skos:inScheme <epos:VolcanoObservations>;
	skos:prefLabel "Hazard maps";
.

