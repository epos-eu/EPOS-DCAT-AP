<https://www.epos-eu.org/epos-dcat-ap/NearFaultObservatory/TABOO/RADON/Dataset/001> a dcat:Dataset;
	dct:title "TABOO Radon decay counts and local temperature Dataset";
	dct:identifier "https://www.epos-eu.org/epos-dcat-ap/NearFaultObservatory/TABOO/RADON/Dataset/001";
	dct:description "Radon decay counts and local temperature dataset contains Radon Gas concentration time series based on periodic counting of Rn gas at site";
	adms:identifier [ a adms:Identifier; 
		adms:schemaAgency "DDSS-ID";
		skos:notation "WP09-DDSS-024"; 
	];
	## example of frequency using a controlled vocabulary
	dct:type "http://purl.org/dc/dcmitype/Collection"^^xsd:anyURI;
	dct:accrualPeriodicity "http://purl.org/cld/freq/continuous"^^xsd:anyURI;
	dct:created "2010-04-01T00:00:00Z"^^xsd:dateTime;
	dct:issued "2010-04-01T00:00:00Z"^^xsd:dateTime;
	dct:modified "2010-04-01T00:00:00Z"^^xsd:dateTime;
	owl:versionInfo "1.0.0";
	# This property refers to a geographic region that is covered by the Dataset.
  	# The POLYGON format is: POLYGON(lon1 lat1, lon2 lat2,...)
	dct:spatial [ a dct:Location ;
		locn:geometry "POLYGON(11.78 43.95, 13.61 43.95, 13.61 42.8, 11.78 42.8, 11.78 43.95)"^^gsp:wktLiteral ;
	];
	# This property refers to a temporal period that the Dataset covers.
    dct:temporal [ a dct:PeriodOfTime ;
        schema:startDate "2010-04-01T00:00:00Z"^^xsd:dateTime ;
        schema:endDate "2018-11-30T00:00:00Z"^^xsd:dateTime ;
    ];
	dcat:theme  <epos:Radonconcentration> ;
	dcat:keyword "Geochemical", "Radon concentration", "Radon decay counts", "geochemistry", "geohazards", "long term continuos timeseries", "local temperature", "NFO", "Near Fault Observatory", "TABOO", "FRIDGE";
	dcat:contactPoint <http://orcid.org/0000-0002-6711-2264/scientificContact>;
	dct:publisher <PIC:999472675>;
	dcat:distribution <https://www.epos-eu.org/epos-dcat-ap/NearFaultObservatory/TABOO/RADON/Distribution/001>;
.

