<https://www.epos-eu.org/epos-dcat-ap/NearFaultObservatory/TABOO/RADON/WebService/001/Operation/001> a hydra:Operation;
	hydra:method "GET"^^xsd:string;
	hydra:returns "application/json";
	hydra:property[ a hydra:IriTemplate;
		hydra:template "http://fridge-services.rm.ingv.it/radon/{?stacode, mintime, maxtime, minlat, maxlat, minlon, maxlon, minperiod, maxperiod, type_site}"^^xsd:string;
		# type of parameter "xsd:string" "xsd:boolean" "xsd:date" " xsd:dateTime" "xsd:decimal" "xsd:double" "xsd:float" "xsd:int" "xsd:integer" "xsd:long"
		hydra:mapping[ a hydra:IriTemplateMapping;
			hydra:variable "stacode"^^xsd:string;
			rdfs:label "Station code";
			rdfs:range "xsd:string";
			http:paramValue "PTRL";
			http:paramValue "MURB";
			http:paramValue "SSFR";
			http:paramValue "CDCA";
			http:paramValue "BADI";
			http:paramValue "BADI2";
			schema:defaultValue "PTRL";
			hydra:required "true"^^xsd:boolean;
		];
		hydra:mapping[ a hydra:IriTemplateMapping;
			hydra:variable "mintime"^^xsd:string;
			rdfs:label "Minimum Time";
			rdfs:range "xsd:dateTime";
			schema:minValue "2010-04-01T00:00:00.000Z";
			schema:maxValue "2017-12-31T00:00:00.000Z";
			schema:defaultValue "2010-05-01T12:00:00.000Z";
			hydra:property "schema:startDate";
      		schema:valuePattern "YYYY-MM-DDThh:mm:ss.SSSZ";
			hydra:required "true"^^xsd:boolean;
		];
		hydra:mapping[ a hydra:IriTemplateMapping;
			hydra:variable "maxtime"^^xsd:string;
			rdfs:label "Maximum Time";
			rdfs:range "xsd:dateTime";
			schema:minValue "2010-04-01T00:00:00.000Z";
			schema:maxValue "2017-12-31T00:00:00.000Z";
			schema:defaultValue "2010-06-01T12:00:00.000Z";
			hydra:property "schema:endDate";
      		schema:valuePattern "YYYY-MM-DDThh:mm:ss.SSSZ";
			hydra:required "true"^^xsd:boolean;
		];
		hydra:mapping[ a hydra:IriTemplateMapping;
			hydra:variable "minlat"^^xsd:string;
			rdfs:label "Minimum Latitude [deg]";
			rdfs:range "xsd:float";
			schema:minValue "-90";
			schema:maxValue "90";
			schema:defaultValue "43.0";
			hydra:property "epos:southernmostLatitude";
			hydra:required "true"^^xsd:boolean;
		];
		hydra:mapping[ a hydra:IriTemplateMapping;
			hydra:variable "maxlat"^^xsd:string;
			rdfs:label "Maximum Latitude [deg]";
			rdfs:range "xsd:float";
			schema:minValue "-90";
			schema:maxValue "90";
			schema:defaultValue "44.0";
			hydra:property "epos:northernmostLatitude";
			hydra:required "true"^^xsd:boolean;
		];
		hydra:mapping[ a hydra:IriTemplateMapping;
			hydra:variable "minlon"^^xsd:string;
			rdfs:label "Minimum Longitude [deg]";
			rdfs:range "xsd:float";
			schema:minValue "-90";
			schema:maxValue "90";
			schema:defaultValue "12.0";
			hydra:property "epos:westernmostLongitude";
			hydra:required "true"^^xsd:boolean;
		];
		hydra:mapping[ a hydra:IriTemplateMapping;
			hydra:variable "maxlon"^^xsd:string;
			rdfs:label "Maximum Longitude [deg]";
			rdfs:range "xsd:float";
			schema:minValue "-90";
			schema:maxValue "90";
			schema:defaultValue "13.0";
			hydra:property "epos:easternmostLongitude";
			hydra:required "true"^^xsd:boolean;
		];
		hydra:mapping[ a hydra:IriTemplateMapping;
			hydra:variable "minperiod"^^xsd:string;
			rdfs:label "Minimum 'sampling period' allowed to extract data [minutes]";
			rdfs:range "xsd:integer";
			schema:minValue "1";
			schema:defaultValue "60";
			hydra:required "true"^^xsd:boolean;
		];
		hydra:mapping[ a hydra:IriTemplateMapping;
			hydra:variable "maxperiod"^^xsd:string;
			rdfs:label "Maximum 'sampling period' allowed to extract data [minutes]";
			rdfs:range "xsd:integer";
			schema:defaultValue "180";
			hydra:required "true"^^xsd:boolean;
		];
		hydra:mapping[ a hydra:IriTemplateMapping;
			hydra:variable "type_site"^^xsd:string;
			rdfs:label "Nickname of type of installation";
			rdfs:range "xsd:string";
			http:paramValue "indoor";
			http:paramValue "shelter";
			http:paramValue "borehole";
			http:paramValue "soil";
			hydra:required "false"^^xsd:boolean;
		];
	] ;
.

