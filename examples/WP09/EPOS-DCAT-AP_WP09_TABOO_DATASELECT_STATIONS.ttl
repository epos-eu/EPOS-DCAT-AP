<https://www.epos-eu.org/epos-dcat-ap/NearFaultObservatory/TABOO/WAVEFORM_CONTINUOUS/WebService/001/Operation/001> a hydra:Operation;
	hydra:method "GET"^^xsd:string;
	hydra:returns "application/vnd.fdsn.mseed";
	hydra:property[ a hydra:IriTemplate;
		hydra:template "http://webservices.ingv.it/fdsnws/dataselect/1/query{?starttime, endtime, network, station, location, channel, quality, minimumlength, nodata}"^^xsd:string;
		hydra:mapping[ a hydra:IriTemplateMapping;
			hydra:variable "starttime"^^xsd:string;
			rdfs:label "Start of the timespan";
			rdfs:range "xsd:dateTime";
			schema:defaultValue "2014-05-10T12:00:00.000Z";
			hydra:property "schema:startDate";
      		schema:valuePattern "YYYY-MM-DDThh:mm:ssZ";
			hydra:required "true"^^xsd:boolean;
		];
		hydra:mapping[ a hydra:IriTemplateMapping;
			hydra:variable "endtime"^^xsd:string;
			rdfs:label "End of the timespan";
			rdfs:range "xsd:dateTime";
			schema:defaultValue "2014-05-10T12:00:02.000Z";
			hydra:property "schema:endDate";
      		schema:valuePattern "YYYY-MM-DDThh:mm:ssZ";
			hydra:required "true"^^xsd:boolean;
		];
		hydra:mapping[ a hydra:IriTemplateMapping;
			hydra:variable "network"^^xsd:string;
			rdfs:label "Network code";
			rdfs:range "xsd:string";
			http:paramValue "IV";
			http:paramValue "TV";
			schema:defaultValue "IV,TV";
			hydra:required "true"^^xsd:boolean;
		];
		hydra:mapping[ a hydra:IriTemplateMapping;
			hydra:variable "station"^^xsd:string;
			rdfs:label "Station code";
			rdfs:range "xsd:string";
			http:paramValue "AOI";
			http:paramValue "APEC";
			http:paramValue "ARVD";
			http:paramValue "ASQU";
			http:paramValue "ASSB";
			http:paramValue "ATBU";
			http:paramValue "ATCC";
			http:paramValue "ATFO";
			http:paramValue "ATLO";
			http:paramValue "ATMC";
