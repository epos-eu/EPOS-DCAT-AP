<https://www.epos-eu.org/epos-dcat-ap/NearFaultObservatory/KOERI/VPVS/WebService/001/Operation/001> a hydra:Operation;
	hydra:method "GET"^^xsd:string;
	hydra:returns "application/json";
	hydra:property[ a hydra:IriTemplate;
		hydra:template "http://fridge-services.rm.ingv.it/vpvs/{?mintime, maxtime, minlat, maxlat, minlon, maxlon, mineqdep, maxeqdep, minnp, minns, maxpw, maxsw, minps, maxgap, midi, maxvpvspw, maxvpvssw, maxherr, maxverr, vpvsmin, codetype, modtype, mettype, maxvpvserr, DIV}"^^xsd:string;
		hydra:mapping[ a hydra:IriTemplateMapping;
			hydra:variable "mintime"^^xsd:string;
			rdfs:label "Minimum Time";
			rdfs:range "xsd:dateTime";
			schema:minValue "2012-01-01T00:00:00.000Z";
			schema:maxValue "2017-01-01T00:00:00.000Z";
			schema:defaultValue "2016-01-01T12:00:00.000Z";
			hydra:property "schema:startDate";
      		schema:valuePattern "YYYY-MM-DDThh:mm:ss.SSSZ";
			hydra:required "true"^^xsd:boolean;
		];
		hydra:mapping[ a hydra:IriTemplateMapping;
			hydra:variable "maxtime"^^xsd:string;
			rdfs:label "Maximum Time";
			rdfs:range "xsd:dateTime";
			schema:minValue "2012-01-01T00:00:00.000Z";
			schema:maxValue "2017-01-01T00:00:00.000Z";
			schema:defaultValue "2016-06-01T12:00:00.000Z";
			hydra:property "schema:endDate";
      		schema:valuePattern "YYYY-MM-DDThh:mm:ss.SSSZ";
			hydra:required "true"^^xsd:boolean;
		];
		hydra:mapping[ a hydra:IriTemplateMapping;
			hydra:variable "minlat"^^xsd:string;
			rdfs:label "Minimum Latitude";
			rdfs:range "xsd:float";
			schema:minValue "-90";
			schema:maxValue "90";
			schema:defaultValue "39.0";
			hydra:property "epos:southernmostLatitude";
			hydra:required "true"^^xsd:boolean;
		];
		hydra:mapping[ a hydra:IriTemplateMapping;
			hydra:variable "maxlat"^^xsd:string;
			rdfs:label "Maximum Latitude";
			rdfs:range "xsd:float";
			schema:minValue "-90";
			schema:maxValue "90";
			schema:defaultValue "42.0";
			hydra:property "epos:northernmostLatitude";
			hydra:required "true"^^xsd:boolean;
		];
		hydra:mapping[ a hydra:IriTemplateMapping;
			hydra:variable "minlon"^^xsd:string;
			rdfs:label "Minimum Longitude";
			rdfs:range "xsd:float";
			schema:minValue "-90";
			schema:maxValue "90";
			schema:defaultValue "26.0";
			hydra:property "epos:westernmostLongitude";
			hydra:required "true"^^xsd:boolean;
		];
		hydra:mapping[ a hydra:IriTemplateMapping;
			hydra:variable "maxlon"^^xsd:string;
			rdfs:label "Maximum Longitude";
			rdfs:range "xsd:float";
			schema:minValue "-90";
			schema:maxValue "90";
			schema:defaultValue "31.0";
			hydra:property "epos:easternmostLongitude";
			hydra:required "true"^^xsd:boolean;
		];
		hydra:mapping[ a hydra:IriTemplateMapping;
			hydra:variable "mineqdep"^^xsd:string;
			rdfs:label "Minimum Earthquake Depth";
			rdfs:range "xsd:float";
			schema:minValue "-1";
			schema:maxValue "100";
			schema:defaultValue "0.4";
			hydra:required "true"^^xsd:boolean;
		];
		hydra:mapping[ a hydra:IriTemplateMapping;
			hydra:variable "maxeqdep"^^xsd:string;
			rdfs:label "Maximum Earhquake Depth";
			rdfs:range "xsd:float";
			schema:minValue "-1";
			schema:maxValue "100";
			schema:defaultValue "5.0";
			hydra:required "true"^^xsd:boolean;
		];
		hydra:mapping[ a hydra:IriTemplateMapping;
			hydra:variable "minnp"^^xsd:string;
			rdfs:label "Number of p-waves onset observations";
			rdfs:range "xsd:integer";
			schema:defaultValue "2";
			hydra:required "true"^^xsd:boolean;
		];
		hydra:mapping[ a hydra:IriTemplateMapping;
			hydra:variable "minns"^^xsd:string;
			rdfs:label "Number of s-waves onset observations";
			rdfs:range "xsd:integer";
			schema:defaultValue "3";
			hydra:required "true"^^xsd:boolean;
		];
		hydra:mapping[ a hydra:IriTemplateMapping;
			hydra:variable "maxpw"^^xsd:string;
			rdfs:label "P-phase onset Hypoinverse standard quality weight";
			rdfs:range "xsd:integer";
			schema:maxValue "4";
			schema:defaultValue "4";
			hydra:required "true"^^xsd:boolean;
		];
		hydra:mapping[ a hydra:IriTemplateMapping;
			hydra:variable "maxsw"^^xsd:string;
			rdfs:label "S-phase onset Hypoinverse standard quality weight";
			rdfs:range "xsd:integer";
			schema:maxValue "4";
			schema:defaultValue "4";
			hydra:required "true"^^xsd:boolean;
		];
		hydra:mapping[ a hydra:IriTemplateMapping;
			hydra:variable "minps"^^xsd:string;
			rdfs:label "Number of P and S couples per earthquake";
			rdfs:range "xsd:integer";
			schema:defaultValue "4";
			hydra:required "true"^^xsd:boolean;
		];
		hydra:mapping[ a hydra:IriTemplateMapping;
			hydra:variable "maxgap"^^xsd:string;
			rdfs:label "Degrees of azimuthal stations distribution gap per earthquake";
			rdfs:range "xsd:integer";
			schema:maxValue "360";
			schema:defaultValue "150";
			hydra:required "true"^^xsd:boolean;
		];
		hydra:mapping[ a hydra:IriTemplateMapping;
			hydra:variable "midi"^^xsd:string;
			rdfs:label "Horizontal distance of the closest station from a earthquake";
			rdfs:range "xsd:float";
			schema:minValue "0.00";
			schema:maxValue "40000.00";
			schema:defaultValue "70";
			hydra:required "true"^^xsd:boolean;
		];
		hydra:mapping[ a hydra:IriTemplateMapping;
			hydra:variable "maxherr"^^xsd:string;
			rdfs:label "Maximum allowed formal horizontal error in earthquake location";
			rdfs:range "xsd:float";
			schema:minValue "0.00";
			schema:maxValue "40000.00";
			schema:defaultValue "300";
			hydra:required "true"^^xsd:boolean;
		];
		hydra:mapping[ a hydra:IriTemplateMapping;
			hydra:variable "maxverr"^^xsd:string;
			rdfs:label "Maximum allowed formal vertical error in earthquake location";
			rdfs:range "xsd:float";
			schema:minValue "0.00";
			schema:maxValue "6378.00";
			schema:defaultValue "200";
			hydra:required "true"^^xsd:boolean;
		];
		hydra:mapping[ a hydra:IriTemplateMapping;
			hydra:variable "maxvpvspw"^^xsd:string;
			rdfs:label "Maximum quality weight allowed for a p-wave reading";
			rdfs:range "xsd:integer";
			schema:minValue "0";
			schema:maxValue "4";
			schema:defaultValue "3";
			hydra:required "false"^^xsd:boolean;
		];
		hydra:mapping[ a hydra:IriTemplateMapping;
			hydra:variable "maxvpvssw"^^xsd:string;
			rdfs:label "Maximum quality weight allowed for a s-wave reading";
			rdfs:range "xsd:integer";
			schema:minValue "0";
			schema:maxValue "4";
			schema:defaultValue "3";
			hydra:required "false"^^xsd:boolean;
		];
		hydra:mapping[ a hydra:IriTemplateMapping;
			hydra:variable "vpvsmin"^^xsd:string;
			rdfs:label "vpvsmin";
			rdfs:range "xsd:float";
			schema:defaultValue "0";
			hydra:required "false"^^xsd:boolean;
		];
		hydra:mapping[ a hydra:IriTemplateMapping;
			hydra:variable "codetype"^^xsd:string;
			rdfs:label "codetype";
			rdfs:range "xsd:integer";
			schema:minValue "0";
			schema:maxValue "4";
			schema:defaultValue "2";
			hydra:required "false"^^xsd:boolean;
		];
		hydra:mapping[ a hydra:IriTemplateMapping;
			hydra:variable "modtype"^^xsd:string;
			rdfs:label "modtype";
			rdfs:range "xsd:integer";
			schema:minValue "0";
			schema:maxValue "4";
			schema:defaultValue "1";
			hydra:required "false"^^xsd:boolean;
		];
		hydra:mapping[ a hydra:IriTemplateMapping;
			hydra:variable "mettype"^^xsd:string;
			rdfs:label "mettype";
			rdfs:range "xsd:integer";
			schema:defaultValue "2";
			hydra:required "false"^^xsd:boolean;
		];
		hydra:mapping[ a hydra:IriTemplateMapping;
			hydra:variable "maxvpvserr"^^xsd:string;
			rdfs:label "maxvpvserr";
			rdfs:range "xsd:integer";
			schema:defaultValue "100000";
			hydra:required "false"^^xsd:boolean;
		];
		hydra:mapping[ a hydra:IriTemplateMapping;
			hydra:variable "DIV"^^xsd:string;
			rdfs:label "DIV";
			rdfs:range "xsd:integer";
			schema:defaultValue "1000000";
			hydra:required "false"^^xsd:boolean;
		];
	] ;
.

