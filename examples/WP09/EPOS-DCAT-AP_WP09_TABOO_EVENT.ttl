<https://www.epos-eu.org/epos-dcat-ap/NearFaultObservatory/TABOO/Event/Dataset/001> a dcat:Dataset;
	dct:identifier "https://www.epos-eu.org/epos-dcat-ap/NearFaultObservatory/TABOO/Event/Dataset/001";
	dct:description "Information about the earthquakes (events) occurred in the Alta Val Tiberina (Central Italy) Near Fault Observatory Area.";
	dct:title "TABOO FDSN Event Dataset";
		adms:identifier [ a adms:Identifier;
		adms:schemaAgency "DDSS-ID";
		skos:notation "WP09-DDSS-006";
	];
	dct:type "http://purl.org/dc/dcmitype/Collection"^^xsd:anyURI;
	dct:accrualPeriodicity "http://purl.org/cld/freq/irregular"^^xsd:anyURI;
	dct:created "2010-04-01"^^xsd:date;
	owl:versionInfo "1.0.0";
	# This property refers to a geographic region that is covered by the Dataset.
  	# The POLYGON format is: POLYGON(lon1 lat1, lon2 lat2,...)
	dct:spatial [ a dct:Location ;
		locn:geometry "POLYGON(11.78 43.95, 13.61 43.95, 13.61 42.8, 11.78 42.8, 11.78 43.95)"^^gsp:wktLiteral ;
	];
	# This property refers to a temporal period that the Dataset covers.
    dct:temporal [ a dct:PeriodOfTime ;
        schema:startDate "2010-04-01T00:00:00"^^xsd:dateTime ;
        #schema:endDate "YYYY-MM-DDThh:mm:ss"^^xsd:dateTime ;
    ];
	dcat:theme  <epos:EarthquakeParameters>;
	dcat:keyword "earthquake", "event", "rocks", "seismic source", "seismic waves propagation", "seismology", "NFO", "Near Fault Observatory", "TABOO", "FRIDGE";
	dcat:contactPoint <http://orcid.org/0000-0003-3489-7453/scientificContact>;
	dct:publisher <PIC:999472675>;
	dcat:distribution <https://www.epos-eu.org/epos-dcat-ap/NearFaultObservatory/TABOO/Event/Distribution/001>;
.

<https://www.epos-eu.org/epos-dcat-ap/NearFaultObservatory/TABOO/Event/Distribution/001> a dcat:Distribution;
