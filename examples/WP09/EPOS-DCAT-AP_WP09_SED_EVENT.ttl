	<http://arclink.ethz.ch/fdsnws/event/1/query?/operation> a hydra:Operation;
		hydra:method "GET"^^xsd:string;
		hydra:returns "http://quakeml.org/xmlns/bed/1.2";
		hydra:property[ a hydra:IriTemplate;
			hydra:template "http://arclink.ethz.ch/fdsnws/event/1/{?starttime, endtime, minlatitude, maxlatitude, minlongitude, maxlongitude, mindepth, maxdepth, minmagnitude, maxmagnitude, includeallorigins, includeallmagnitudes, includearrivals, eventid, limit, offset, orderby, contributor, catalog, updatedafter, format, nodata, latitude, longitude, minradius, maxradius}"^^xsd:string;
				hydra:mapping[ a hydra:IriTemplateMapping;
					hydra:variable "starttime"^^xsd:string;
					rdfs:range "xsd:datetime";
					rdfs:label "start time";
				        hydra:property "schema:startDate";
					hydra:required "false"^^xsd:boolean;
					schema:valuePattern "YYYY-MM-DDThh:mm:ss";
				];
				hydra:mapping[ a hydra:IriTemplateMapping;
					hydra:variable "endtime"^^xsd:string;
					rdfs:range "xsd:datetime";
					rdfs:label "end time";
					hydra:property "schema:endDate";
                                        schema:valuePattern "YYYY-MM-DDThh:mm:ss";
					hydra:required "false"^^xsd:boolean;
				];
				hydra:mapping[ a hydra:IriTemplateMapping;
					hydra:variable "minlatitude"^^xsd:string;
					rdfs:range "xsd:float";
					rdfs:label "minimum Latitude";
					schema:minValue "-90";
					schema:maxValue "90";
					hydra:required "false"^^xsd:boolean;
					hydra:variable "minlatitude"^^xsd:string ;
					hydra:property "epos:southernmostLatitude" ;
				];
				hydra:mapping[ a hydra:IriTemplateMapping;
					hydra:variable "maxlatitude"^^xsd:string;
					rdfs:range "xsd:float";
					rdfs:label "max Latitude";
					schema:minValue "-90";
					schema:maxValue "90";
					hydra:required "false"^^xsd:boolean;
					hydra:property "epos:northernmostLatitude" ;
				];
				hydra:mapping[ a hydra:IriTemplateMapping;
					hydra:variable "minlongitude"^^xsd:string;
					rdfs:range "xsd:float";
					rdfs:label "minimum Longitude";
					schema:minValue "-90";
					schema:maxValue "90";
					hydra:required "false"^^xsd:boolean;
					hydra:property "epos:westernmostLongitude" ;
				];
				hydra:mapping[ a hydra:IriTemplateMapping;
					hydra:variable "maxlongitude"^^xsd:string;
					rdfs:range "xsd:float";
					rdfs:label "max Longitude";
					schema:minValue "-90";
					schema:maxValue "90";
					hydra:required "false"^^xsd:boolean;
					hydra:property "epos:easternmostLongitude" ;
				];
				hydra:mapping[ a hydra:IriTemplateMapping;
					hydra:variable "mindepth"^^xsd:string;
					rdfs:range "xsd:int";
					rdfs:label "minimum Depth";
					schema:minValue "-1";
					schema:maxValue "100";
					hydra:required "false"^^xsd:boolean;
				];
				hydra:mapping[ a hydra:IriTemplateMapping;
					hydra:variable "maxdepth"^^xsd:string;
					rdfs:range "xsd:int";
					rdfs:label "max Depth";
					schema:minValue "-1";
					schema:maxValue "100";
					hydra:required "false"^^xsd:boolean;
				];
				hydra:mapping[ a hydra:IriTemplateMapping;
					hydra:variable "minmagnitude"^^xsd:string;
					rdfs:range "xsd:float";
					rdfs:label "minimum Magnitude";
					schema:maxValue "10";
					hydra:required "false"^^xsd:boolean;
				];
				hydra:mapping[ a hydra:IriTemplateMapping;
					hydra:variable "maxmagnitude"^^xsd:string;
					rdfs:range "xsd:float";
					rdfs:label "max Magnitude";
					schema:maxValue "10";
					hydra:required "false"^^xsd:boolean;
				];
				hydra:mapping[ a hydra:IriTemplateMapping;
					hydra:variable "includeallorigins"^^xsd:string;
					rdfs:range "xsd:boolean";
					rdfs:label "include all Origin";
					hydra:required "false"^^xsd:boolean;
				];
				hydra:mapping[ a hydra:IriTemplateMapping;
					hydra:variable "includeallmagnitudes"^^xsd:string;
					rdfs:range "xsd:boolean";
					rdfs:label "include all mag";
					hydra:required "false"^^xsd:boolean;
				];
				hydra:mapping[ a hydra:IriTemplateMapping;
					hydra:variable "includearrivals"^^xsd:string;
					rdfs:range "xsd:boolean";
					rdfs:label "include all arrivals";
					hydra:required "false"^^xsd:boolean;
				];
				hydra:mapping[ a hydra:IriTemplateMapping;
					hydra:variable "eventid"^^xsd:string;
					rdfs:range "xsd:string";
					rdfs:label "Event ID";
					hydra:required "false"^^xsd:boolean;
				];
				hydra:mapping[ a hydra:IriTemplateMapping;
					hydra:variable "limit"^^xsd:string;
					rdfs:range "xsd:int";
					rdfs:label "limit";
					hydra:required "false"^^xsd:boolean;
				];
				hydra:mapping[ a hydra:IriTemplateMapping;
					hydra:variable "offset"^^xsd:string;
					rdfs:range "xsd:int";
					rdfs:label "offset";
					hydra:required "false"^^xsd:boolean;
				];
				hydra:mapping[ a hydra:IriTemplateMapping;
					hydra:variable "orderby"^^xsd:string;
					rdfs:range "xsd:int";
					rdfs:label "orderby";
					hydra:required "false"^^xsd:boolean;
				];
				hydra:mapping[ a hydra:IriTemplateMapping;
					hydra:variable "contributor"^^xsd:string;
					rdfs:range "xsd:string";
					rdfs:label "contributor";
					hydra:required "false"^^xsd:boolean;
				];
				hydra:mapping[ a hydra:IriTemplateMapping;
					hydra:variable "catalog"^^xsd:string;
					rdfs:range "xsd:string";
					rdfs:label "Catalogue";
					hydra:required "false"^^xsd:boolean;
				];
				hydra:mapping[ a hydra:IriTemplateMapping;
					hydra:variable "updatedafter"^^xsd:string;
					rdfs:range "xsd:datetime";
					rdfs:label "is update after";
					schema:minValue "2001-12-31T12:00:00";
					schema:maxValue "2017-06-20T12:00:00";
					hydra:required "false"^^xsd:boolean;
				];
				hydra:mapping[ a hydra:IriTemplateMapping;
					hydra:variable "format"^^xsd:string;
					rdfs:range "xsd:string";
					rdfs:label "output format";
					http:paramValue "xml";
					http:paramValue "kml";
					http:paramValue "txt";
					hydra:required "false"^^xsd:boolean;
				];
				hydra:mapping[ a hydra:IriTemplateMapping;
					hydra:variable "nodata"^^xsd:string;
					rdfs:range "xsd:string";
					rdfs:label "no data";
					hydra:required "false"^^xsd:boolean;
				];
				hydra:mapping[ a hydra:IriTemplateMapping;
					hydra:variable "latitude"^^xsd:string;
					rdfs:range "xsd:float";
					rdfs:label "Latitude";
					schema:minValue "-90";
					schema:maxValue "90";
					hydra:required "false"^^xsd:boolean;
				];
				hydra:mapping[ a hydra:IriTemplateMapping;
					hydra:variable "longitude"^^xsd:string;
					rdfs:range "xsd:float";
					rdfs:label "Longitude";
					schema:minValue "-90";
					schema:maxValue "90";
					hydra:required "false"^^xsd:boolean;
				];
				hydra:mapping[ a hydra:IriTemplateMapping;
					hydra:variable "minradius"^^xsd:string;
					rdfs:range "xsd:float";
					rdfs:label "minradius";
					schema:minValue "-90";
					schema:maxValue "90";
					hydra:required "false"^^xsd:boolean;
				];
				hydra:mapping[ a hydra:IriTemplateMapping;
					hydra:variable "maxradius"^^xsd:string;
					rdfs:range "xsd:float";
					rdfs:label "maxradius";
					schema:minValue "-90";
					schema:maxValue "90";
					hydra:required "false"^^xsd:boolean;
				];
		] ;
. 
