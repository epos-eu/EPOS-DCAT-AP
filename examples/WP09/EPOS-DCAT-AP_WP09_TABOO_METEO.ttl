<https://www.epos-eu.org/epos-dcat-ap/NearFaultObservatory/TABOO/METEO/Dataset/001> a dcat:Dataset;
	dct:title "TABOO Meteorological Parameters at CO2 site Dataset";
	dct:identifier "https://www.epos-eu.org/epos-dcat-ap/NearFaultObservatory/TABOO/METEO/Dataset/001";
	dct:description "Meteorological parameters at CO2 site Web Service retrieve meteorological parameters time series based on periodic counting of water level, water conductivity, air temperature, water temperature, rain, humidity, atmospheric pressure at CO2 site";
	adms:identifier [ a adms:Identifier; 
		adms:schemaAgency "DDSS-ID";
		skos:notation "WP09-DDSS-025";
	];
	## example of frequency using a controlled vocabulary
	dct:type "http://purl.org/dc/dcmitype/Collection"^^xsd:anyURI;
	dct:accrualPeriodicity "http://purl.org/cld/freq/continuous"^^xsd:anyURI;
	dct:created "2017-03-01T00:00:00Z"^^xsd:dateTime;
	dct:issued "2017-03-01T00:00:00Z"^^xsd:dateTime;
	dct:modified "2017-03-01T00:00:00Z"^^xsd:dateTime;
	owl:versionInfo "1.0.0";
	# This property refers to a geographic region that is covered by the Dataset.
  	# The POLYGON format is: POLYGON(lon1 lat1, lon2 lat2,...)
	dct:spatial [ a dct:Location ;
		locn:geometry "POLYGON((11.78 43.95, 13.61 43.95, 13.61 42.8, 11.78 42.8, 11.78 43.95))"^^gsp:wktLiteral ;
	];
	# This property refers to a temporal period that the Dataset covers.
    dct:temporal [ a dct:PeriodOfTime ;
        schema:startDate "2015-06-01T00:00:00Z"^^xsd:dateTime ;
        # schema:endDate "2019-01-01T00:00:00Z"^^xsd:dateTime ;
    ];
	dcat:theme  <epos:Meteorologicaldata>;
	dcat:keyword "geochemistry", "meteo parameter", "meteo parameters at CO2 site", "air temperature", "rain", "humidity", "atmospheric pressure", "NFO", "TABOO", "FRIDGE";
	dcat:contactPoint <http://orcid.org/0000-0002-8608-4289/contactPoint>;
	dct:publisher <PIC:999472675>;
    dcat:distribution <https://www.epos-eu.org/epos-dcat-ap/NearFaultObservatory/TABOO/METEO/Distribution/001>;
.

<https://www.epos-eu.org/epos-dcat-ap/NearFaultObservatory/TABOO/METEO/Distribution/001> a dcat:Distribution;
    dct:identifier "https://www.epos-eu.org/epos-dcat-ap/NearFaultObservatory/TABOO/METEO/Distribution/001";
	dct:title "TABOO Meteo Parameters at CO2 site";
	dct:description "This distribution provides access to time series of meteo parameters like air temperature, rain, humidity and atmospheric pressure measured at CO2 sites. The datasets are encoded in JSON format. Data cover the time period 2010-today and they are limited to the Alta Val Tiberina (Central Italy) Near Fault Observatory.";
	dct:created "2017-03-01"^^xsd:date;
	dct:type "http://publications.europa.eu/resource/authority/distribution-type/WEB_SERVICE"^^xsd:anyURI;
	dcat:accessURL <https://www.epos-eu.org/epos-dcat-ap/NearFaultObservatory/TABOO/METEO/WebService/001/Operation/001>;
	dct:conformsTo <https://www.epos-eu.org/epos-dcat-ap/NearFaultObservatory/TABOO/METEO/WebService/001>;
	dct:format "http://publications.europa.eu/resource/authority/file-type/JSON"^^xsd:anyURI;
	# This property refers to the licence under which the Distribution is made available.
	dct:license "http://creativecommons.org/licenses/by/4.0/"^^xsd:anyURI ;
.

