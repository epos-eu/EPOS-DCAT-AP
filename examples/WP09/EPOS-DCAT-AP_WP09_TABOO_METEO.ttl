<https://www.epos-eu.org/epos-dcat-ap/NearFaultObservatory/TABOO/METEO/WebService/001/Operation/001> a hydra:Operation;
	hydra:method "GET"^^xsd:string;
	hydra:returns "application/json";
	hydra:property[ a hydra:IriTemplate;
		hydra:template "http://fridge-services.rm.ingv.it/meteo/{?param_name, mintime, maxtime, minlat, maxlat, minlon, maxlon, stacode}"^^xsd:string;
		# type of parameter "xsd:string" "xsd:boolean" "xsd:date" "xsd:dateTime" "xsd:decimal" "xsd:double" "xsd:float" "xsd:int" "xsd:integer" "xsd:long"
		hydra:mapping[ a hydra:IriTemplateMapping;
			hydra:variable "param_name"^^xsd:string;
			rdfs:label "Meteorological parameter at CO2 site";
			rdfs:range "xsd:string";
			http:paramValue "water_level";
			http:paramValue "air_temperature_ext";
			http:paramValue "air_temperature_int";
			http:paramValue "water_temperature";
			http:paramValue "water_conductivity";
			http:paramValue "rain";
			http:paramValue "humidity";
			http:paramValue "atmospheric_pressure";
			schema:defaultValue "humidity";
			hydra:required "true"^^xsd:boolean;
		];
		hydra:mapping[ a hydra:IriTemplateMapping;
			hydra:variable "mintime"^^xsd:string;
			rdfs:label "Minimum time";
			rdfs:range "xsd:dateTime";
			schema:minValue "2015-06-01T00:00:00.000Z";
			schema:maxValue "2017-12-31T00:00:00.000Z";
			schema:defaultValue "2016-12-01T00:00:00.000Z";
			hydra:property "schema:startDate";
      		schema:valuePattern "YYYY-MM-DDThh:mm:ss.SSSZ";
			hydra:required "true"^^xsd:boolean;
		];
		hydra:mapping[ a hydra:IriTemplateMapping;
			hydra:variable "maxtime"^^xsd:string;
			rdfs:label "Maximum time";
			rdfs:range "xsd:dateTime";
			schema:minValue "2015-06-01T00:00:00.000Z";
			schema:maxValue "2017-12-31T00:00:00.000Z";
			schema:defaultValue "2016-12-31T23:59:00.000Z";
			hydra:property "schema:endDate";
      		schema:valuePattern "YYYY-MM-DDThh:mm:ss.SSSZ";
			hydra:required "true"^^xsd:boolean;
		];
		hydra:mapping[ a hydra:IriTemplateMapping;
			hydra:variable "minlat"^^xsd:string;
			rdfs:label "Minimum Latitude [deg]";
			rdfs:range "xsd:float";
			schema:minValue "-90";
			schema:maxValue "90";
			schema:defaultValue "43.0";
			hydra:property "epos:southernmostLatitude";
			hydra:required "true"^^xsd:boolean;
		];
		hydra:mapping[ a hydra:IriTemplateMapping;
			hydra:variable "maxlat"^^xsd:string;
			rdfs:label "Maximum Latitude [deg]";
			rdfs:range "xsd:float";
			schema:minValue "-90";
			schema:maxValue "90";
			schema:defaultValue "44.0";
			hydra:property "epos:northernmostLatitude";
			hydra:required "true"^^xsd:boolean;
		];
		hydra:mapping[ a hydra:IriTemplateMapping;
			hydra:variable "minlon"^^xsd:string;
			rdfs:label "Minimum Longitude [deg]";
			rdfs:range "xsd:float";
			schema:minValue "-90";
			schema:maxValue "90";
			schema:defaultValue "12.0";
			hydra:property "epos:westernmostLongitude";
			hydra:required "true"^^xsd:boolean;
		];
		hydra:mapping[ a hydra:IriTemplateMapping;
			hydra:variable "maxlon"^^xsd:string;
			rdfs:label "Maximum Longitude [deg]";
			rdfs:range "xsd:float";
			schema:minValue "-90";
			schema:maxValue "90";
			schema:defaultValue "13.0";
			hydra:property "epos:easternmostLongitude";
			hydra:required "true"^^xsd:boolean;
		];
		hydra:mapping[ a hydra:IriTemplateMapping;
			hydra:variable "stacode"^^xsd:string;
			rdfs:label "Station code";
			rdfs:range "xsd:string";
			http:paramValue "nogna";
			http:paramValue "uppiano";
			http:paramValue "migianella";
			http:paramValue "fungaia";
			schema:defaultValue "uppiano";
			hydra:required "true"^^xsd:boolean;
		];
	] ;
.
